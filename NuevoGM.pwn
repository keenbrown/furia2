					Portales[i][p@PORT2] = CreateDynamicObject(19902,pos[0],pos[1],pos[2]-1.2,0.00000000,0.00000000,0.00000000);
					SetPlayerVirtualWorld(playerid, Portales[i][p@world]);
					Portales[i][p@usado] = 1;
					EnEntrada[playerid] = 0;
					GuardarPortales(i);
					return true;
				}
			}
	 	}	ShowError(playerid,"Se alzando el limite de PORTS.");
