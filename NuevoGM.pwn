COMMAND:conquistar(playerid, params[])return cmd_capturar(playerid, params);
COMMAND:capturar(playerid,params[])
{// /capturar
	if(tieneClan(playerid))
	{
		new zoneid = UsuarioEnZona(playerid);
		if(zoneid == 0)
		{
			SendClientMessage(playerid,COLOR_ROJO,"Necesitas estar en una zona conquistable.");
			return 1;
		}
		else
		{
			new t = Clan[playerid][Miembro], z = zoneid;
			if(Clanes[t][aSlot] == 1)
			{
				if(Zonas[z][Existe] == 1)
				{
					if(z == zoneid)
					{
						if(Zonas[z][Conquistando] == 1)
						{
							SendClientMessage(playerid,COLOR_ROJO,"Esta zona ya esta siendo capturada.");
							return 1;
						}
						else
						{
							if(ObtenerMiembrosEnZona(z,t) < MAX_MIEMBROS_CONQUISTA)
							{
								SendClientMessage(playerid,COLOR_ROJO,"Necesitan estar 2 miembros en la zona.");
								return 1;
							}
							else
							{
								if(Zonas[z][Dueno] == Clan[playerid][Miembro])
								{
									SendClientMessage(playerid,COLOR_ROJO,"Esta zona ya pertenece a tu clan.");
									return 1;
								}
								else
								{
									new tmp[126];
									if(TiemposLimites[playerid][Ya@Conquisto] > gettime())
									{
										mensaje_format(tmp,playerid,COLOR_BLANCO,"{FFFFFF}Espera ({FFFFCC}%s{FFFFFF}) para usar nuevamente este beneficio.", obtenertiempo(TiemposLimites[playerid][Ya@Conquisto]));
										return true;
										}
											TiemposLimites[playerid][Ya@Conquisto] = gettime() + 400;
											GangZoneFlashForAll(Zonas[z][ID],coloresclanlista[Clanes[Clan[playerid][Miembro]][aColor]]);
											Zonas[z][Conquistando] = 1;
											Zonas[z][Tiempo@restante@conquista] = gettime() + Zonas[z][Tiempoconquista] * 60;
											Zonas[z][Conquistador@actual] = t;
											new Float:uPos[3];
											GetPlayerPos(playerid,uPos[0],uPos[1],uPos[2]);
											foreach(new i : Player)
											{
											if(IsPlayerConnected(i) && Clan[i][Miembro] == Clan[playerid][Miembro])
											{
												if(UsuarioEnZona(i) == zoneid)
												{
													PlayerTextDrawShow(i,Zonas@Tiempo@Restante@Conquista);
													Zona@Time@Restante[i] = z;
												}
											}
										}
										if(Zonas[zoneid][Dueno] !=0)
										{
											foreachEx(pi:MAX_PLAYERS)
											{
											if(IsPlayerConnected(pi) && Zonas[zoneid][Dueno] == Clan[pi][Miembro])
											{
												SendClientMessageEx(pi,COLOR_AMARILLO,"« CLAN » {ffffff} El clan {ffffff}[%s] esta capturando tu zona "coloramarillo"[%s]{ffffff} debes impedirlo.",Clanes[t][aNombre],Zonas[zoneid][Ubicacion]);
											}
										}
									}//Zonas[zoneid][Ubicacion]
									SendClientMessageEx(-1,coloresclanlista[Clanes[Clan[playerid][Miembro]][aColor]],"« ZONAS » {FFFFFF} El clan %s esta capturando la zona %s - "colorkaki"[Zonas: %d/%d]",Clanes[Clan[playerid][Miembro]][aNombre],Zonas[zoneid][Ubicacion],Clanes[Clan[playerid][Miembro]][aConquista],ContarZonas());
									TimerZona[zoneid] = SetTimerEx("Update@CapturaZone",Zonas[z][Tiempoconquista]*60000,false,"dd",zoneid,t);
								}
							}
						}
					}
				}
			}
		}
	}else return SendClientMessage(playerid,COLOR_ROJO,"No perteneces a un clan.");
	return 1;
}

