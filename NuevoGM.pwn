//=== System GPS By Pepe =====================================================//
stock Float:GDBP(Float:X, Float:Y, Float:Z, Float:PointX, Float:PointY, Float:PointZ)
    return (floatsqroot(floatadd(floatadd(floatpower(floatsub(X, PointX), 2.0), floatpower(floatsub(Y, PointY), 2.0)), floatpower(floatsub(Z, PointZ), 2.0))));

stock SetPlayerGarmin(playerid, Float:x, Float:y, Float:z, color = 0x8A44E4FF)
{
//	if(IsPlayerInAnyVehicle(playerid)) return true;
	DestroyGarminRoutesGPS(playerid);	StopGPSPepe(playerid);
	DisablePlayerRaceCheckpoint(playerid);
	GPSPepe[playerid][GPSPepe_Color] = color;
	GPSPepe[playerid][GPSPepe_Position][0] = x;
	GPSPepe[playerid][GPSPepe_Position][1] = y;
	GPSPepe[playerid][GPSPepe_Position][2] = z;

	if(GPSPepe[playerid][GPSPepe_Timer] == 0)GPSPepe[playerid][GPSPepe_Timer] = SetTimerEx("UpdateGarmin", GARMIN_UPDATE_TIME, true, "ifff", playerid, x, y, z);
	UpdateGarmin(playerid, x, y, z);		// Force First Update
	SetPlayerRaceCheckpoint(playerid, 2, x, y, z, 0.0,0.0,0.0, 7.0);
	return true;
}

