CMD:camion(playerid, params[])
{
    if(isnull(params)) return SendClientMessage(playerid, 0xE88732FF, "USAGE: {FFFFFF}/camion [iniciar/cancelar/cargar/despachar/carga]");
    if(!strcmp(params, "iniciar", true))
	{
		new vehicleid = GetPlayerVehicleID(playerid);// Obtener el vehículo del jugador
 		if(!IsValidVehicle(vehicleid)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No estás dentro de un camion.");
        if(!Vehicle_PetrolVehicle(vehicleid)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}El vehículo no es un vehículo Petrolero.");
		if(g_PlayerTrailerState[playerid] != 1) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}Toma un Trailer para Comenzar.");
		SendClientMessage(playerid, 0x2ECC71FF, "PETROLERA: {FFFFFF}Ve y carga el camion.");
		SetPlayerGarmin(playerid,ZOil[0][zP@x],ZOil[0][zP@y],ZOil[0][zP@z], 0xFF66FFFF);
		ShowPlayerMessage(playerid,"~g~~h~~h~Localizando~w~ Deposito", 5);
		Ubicacion@Gasolinera[playerid] = true;
	}
 	else if(!strcmp(params, "cancelar", true))
	{
		if(IsPlayerInRangeOfPoint(playerid, 10.0, 2195.04248, -2255.49170, 12.53622)) return ShowPlayerMessage(playerid, "Debes estar en la Zona de Camiones para Terminar!", 3);
		Ubicacion@Gasolinera[playerid] = false;	CamionLleno[playerid] = false;
 		DisablePlayerCheckpoint(playerid); 	StopGPSPepe(playerid);
	 	new vehicleid = GetPVarInt(playerid, "LastVehicleID");
	 	new trailerid = GetVehicleTrailer(vehicleid);// Verificar si el vehículo tiene un remolque
	 	SetVehicleToRespawn(vehicleid);SetVehicleHealth(vehicleid, 1000.0);SetVehicleParamsEx(vehicleid,0,0,0,0,0,0,0);	SetVehicleParamsCarWindows(vehicleid, 0, 0, 0, 0);
	 	SetVehicleToRespawn(trailerid);SetVehicleHealth(trailerid, 1000.0);SetVehicleParamsEx(trailerid,0,0,0,0,0,0,0);	SetVehicleParamsCarWindows(trailerid, 0, 0, 0, 0);
	}
    else if(!strcmp(params, "cargar", true))
	{
		foreachEx(i:sizeof(ZOil))
		{
			if(ZOil[i][zP@usado])
			{
				if (IsPlayerInRangeOfPoint(playerid,15.0,ZOil[i][zP@x],ZOil[i][zP@y],ZOil[i][zP@z]))
				{
					if(ZOil[i][zP@Cant] < 10)return ShowError(playerid,"Este Deposito esta Vacio.");
					new vehicleid = GetPVarInt(playerid, "LastVehicleID");
	    			new vehicleid2 = GetNearestVehicleEx(playerid);// vehiculo cercano
	    			new Float: x, Float: y, Float: z;
					GetVehicleBoot(vehicleid2, x, y, z);
					if(GetPlayerDistanceFromPoint(playerid, x, y, z) > 5.0) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No estás cerca de las mangueras del Trailer.");
			        if(g_PlayerTrailerState[playerid] != 1) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No Tienes Trailer de Carga.");
			        if(!IsValidVehicle(vehicleid)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No estás cerca de ningún vehículo.");
			        if(!Vehicle_TrailerVehicle(vehicleid2)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}El vehículo no es un vehículo Petrolero.");
					if(Vehicle_LoadedPetrol(vehicleid2) >= OIL_LIMIT) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No puedes cargar más Combustible en este vehículo.");
					LoadedPetrol[vehicleid2] +=10000;
					ZOil[i][zP@Cant] -= 10000;
					GuardarZonaOil(i);
	    			new string[128];
		        	format(string, sizeof(string), "PETROLERA: {FFFFFF}Este vehículo tiene {F39C12}%d {FFFFFF}combustible cargado, su limite es {2ECC71}%d.", Vehicle_LoadedPetrol(vehicleid2), OIL_LIMIT);
					SendClientMessage(playerid, 0x2ECC71FF, string);
					SetCamioneroCheckpoint(playerid);
				}else return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No Estas en una Petrolera.");
				return 1;
			}
		}
    }
    else if(!strcmp(params, "carga", true))
	{
		new vehicleid, trailerid;
	   	vehicleid = GetPlayerVehicleID(playerid);// Obtener el vehículo del jugador
	    trailerid = GetVehicleTrailer(vehicleid);// Verificar si el vehículo tiene un remolque
        if(g_PlayerTrailerState[playerid] != 1) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No Tienes Trailer de Carga.");
        if(!IsValidVehicle(vehicleid)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No estás dentro de un camion.");
        if(!Vehicle_PetrolVehicle(vehicleid)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}El vehículo no es un vehículo Petrolero.");
        new string[196], title[125];
		format(string, sizeof(string), "Nombre\tCantidad\tValor\n");
        format(string, sizeof(string), "%s%s\t%d\t{2ECC71}$%d\n", string,"Gasolina" ,LoadedPetrol[trailerid], Vehicle_LoadedPetrol(trailerid)/4);
		format(title, sizeof(title), "Combustible Cargado {E74C3C}(%d / %d)", Vehicle_LoadedPetrol(trailerid), OIL_LIMIT);
		ShowPlayerDialog(playerid, DIALOGO_UNO, DIALOG_STYLE_TABLIST_HEADERS, title, string, "Cerrar", "");
    }
	else if(!strcmp(params, "despachar"))
	{
		new id = GetNearestVehicleEx(playerid);
		if (GetVehicleModel(id) == 584) // 411 is the Infernus model
	    {
  	 		new Float: x, Float: y, Float: z, string[196], title[125];
			GetVehicleBoot(id, x, y, z);
			if(GetPlayerDistanceFromPoint(playerid, x, y, z) > 5.0) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No estás cerca de las mangueras del Trailer.");
			if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}Bajate del vehículo para descargar.");
			format(string, sizeof(string), "Nombre\tCantidad\n");
	        format(string, sizeof(string), "%s%s\t%d\n", string,"Gasolina" ,LoadedPetrol[id]);
			format(title, sizeof(title), "Combustible Cargado {E74C3C}(%d / %d)", Vehicle_LoadedPetrol(id), OIL_LIMIT);
			ShowPlayerDialog(playerid, DIALOGO_COMBUSTIBLE, DIALOG_STYLE_TABLIST_HEADERS, title, string, "Retirar", "Cerrar");
		}else return SendClientMessage(playerid, 0xE74C3CFF, "ERROR: {FFFFFF}No hay un Trailer de Carga Cerca.");
    }return 1;
}

