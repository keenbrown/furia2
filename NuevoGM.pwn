COMMAND:crearautojob(playerid, args[])
{
	if(!esFundador(playerid))return ShowError(playerid, "Sin permisos para usar este comando.");
	new model,job, tmp[164], DBResult:result, col01, col02;
	if(sscanf(args, "dddd",job, model, col01, col02))return ShowError(playerid, "Use: /crearauto [Auto Job ID][Auto Modelo ID][Color 1][Color 2]");
	if(!(400 <= model <= 611))return MensajeError(playerid, "El ID no es valido.");
	if(!(0 <= col01 <= 255))return ShowError(playerid, "Solo existen 255 colores.");
	if(!(0 <= col02 <= 255))return ShowError(playerid, "Solo existen 255 colores.");
	for(new i=0;i<sizeof(AutosJobs);i++)
	{
		if(!AutosJobs[i][aJob_uso])
		{
			AutosJobs[i][aJob_uso] = 1;
			AutosJobs[i][aJob_job] = job;
			AutosJobs[i][aJob_modelo] = model;
			AutosJobs[i][aJob_color][0] = col01;
			AutosJobs[i][aJob_color][1] = col02;
			GetPlayerPos(playerid, AutosJobs[i][aJob_posicion][0], AutosJobs[i][aJob_posicion][1], AutosJobs[i][aJob_posicion][2]);
			GetPlayerFacingAngle(playerid, AutosJobs[i][aJob_posicion][3]);
