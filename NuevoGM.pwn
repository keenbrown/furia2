Funcion.CompraVehiculoPers(playerid, model, price)
{
	if(GetOwnedVehicle(playerid) != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_ROJO, "SERVIDOR: Ya tienes un vehículo, vendelo para comprar otro.");
    if(GetPlayerMoney(playerid) < price)
	{
	    new string[128];
	    format(string, sizeof(string), "SERVER: No tienes $%d. para comprar este Vehiculo", price);
		return SendClientMessage(playerid, COLOR_ROJO, string);
	}
	PlayerData[playerid][Dinero] -= price;

	new Float:pos[4];
	GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
	GetPlayerFacingAngle(playerid, pos[3]);

	AutoPers = CreateVehicle(model, pos[0], pos[1], pos[2], pos[3], random(255), random(255), -1);
	SetVehicleNumberPlate(AutoPers, Nombre(playerid));
	SetVehicleToRespawn(AutoPers);
	SetVehicleHealth(AutoPers, 1000.0);
	SetVehicleParamsEx(AutoPers,1,1,0,0,0,0,0);
	SetVehicleParamsCarWindows(AutoPers, 1, 1, 1, 1);

	PutPlayerInVehicle(playerid, AutoPers, 0);

	VehicleInfo[AutoPers][vehicle_owner] = Nombre(playerid);
	VehicleInfo[AutoPers][vehicle_model] = model;
	VehicleInfo[AutoPers][vehicle_x] = pos[0];
	VehicleInfo[AutoPers][vehicle_y] = pos[1];
	VehicleInfo[AutoPers][vehicle_z] = pos[2];
	VehicleInfo[AutoPers][vehicle_a] = pos[3];
	VehicleInfo[AutoPers][vehicle_health] = 1000.00;
	VehicleInfo[AutoPers][vehicle_colour1] = random(255);
	VehicleInfo[AutoPers][vehicle_colour2] = random(255);
	VehicleInfo[AutoPers][vehicle_paintjob] = 0;
	VehicleInfo[AutoPers][vehicle_plate] = Nombre(playerid);
	VehicleInfo[AutoPers][vehicle_price] = price;
	VehicleInfo[AutoPers][EMod1] = 0;
	VehicleInfo[AutoPers][EMod2] = 0;
	VehicleInfo[AutoPers][EMod3] = 0;
	VehicleInfo[AutoPers][EMod4] = 0;
	VehicleInfo[AutoPers][EMod5] = 0;
	VehicleInfo[AutoPers][EMod6] = 0;
	VehicleInfo[AutoPers][EMod7] = 0;
	VehicleInfo[AutoPers][EMod8] = 0;
	VehicleInfo[AutoPers][EMod9] = 0;
	VehicleInfo[AutoPers][EMod10] = 0;
	VehicleInfo[AutoPers][EMod11] = 0;
	VehicleInfo[AutoPers][EMod12] = 0;
	VehicleInfo[AutoPers][EMod13] = 0;
	VehicleInfo[AutoPers][EMod14] = 0;
	VehicleInfo[AutoPers][EMod15] = 0;
	VehicleInfo[AutoPers][EMod16] = 0;
	VehicleInfo[AutoPers][vehicle_locked] = 0;
	VehicleInfo[AutoPers][vehicle_Fuell] = 100;

	new query[500];
	format(query, sizeof(query), "INSERT INTO `VehPers`(`DUENO`,`MODEL`,`COLOUR1`,`COLOUR2`,`PRICE`,`X`,`Y`,`Z`,`A`,`HEALTH`,`PLATE`,`PAINTJOB`,`Mod1`,`Mod2`,`Mod3`,`Mod4`,`Mod5`,`Mod6`,`Mod7`,`Mod8`,`Mod9`,`Mod10`,`Mod11`,`Mod12`,`Mod13`,`Mod14`,`Mod15`,`Mod16`,`LOCKED`,`FUELL`) VALUES ('%s', '%d', '%d', '%d', '%d', '%f', '%f', '%f', '%f', '%f', '%s', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%d')",
	DB_Escape(Nombre(playerid)), VehicleInfo[AutoPers][vehicle_model], VehicleInfo[AutoPers][vehicle_colour1], VehicleInfo[AutoPers][vehicle_colour2], VehicleInfo[AutoPers][vehicle_price], VehicleInfo[AutoPers][vehicle_x], VehicleInfo[AutoPers][vehicle_y], VehicleInfo[AutoPers][vehicle_z], VehicleInfo[AutoPers][vehicle_a],
 	VehicleInfo[AutoPers][vehicle_health], VehicleInfo[AutoPers][vehicle_plate], VehicleInfo[AutoPers][vehicle_paintjob],VehicleInfo[AutoPers][EMod1],VehicleInfo[AutoPers][EMod2],VehicleInfo[AutoPers][EMod3],VehicleInfo[AutoPers][EMod4], VehicleInfo[AutoPers][EMod5],VehicleInfo[AutoPers][EMod6],VehicleInfo[AutoPers][EMod7],VehicleInfo[AutoPers][EMod8],
 	VehicleInfo[AutoPers][EMod9],VehicleInfo[AutoPers][EMod10],VehicleInfo[AutoPers][EMod11],VehicleInfo[AutoPers][EMod12], VehicleInfo[AutoPers][EMod13],VehicleInfo[AutoPers][EMod14],VehicleInfo[AutoPers][EMod15],VehicleInfo[AutoPers][EMod6],VehicleInfo[AutoPers][vehicle_locked],VehicleInfo[AutoPers][vehicle_Fuell]);
