	format(Query,sizeof(Query),"SELECT `ID` FROM `Zonas` WHERE `ID` = '%d' COLLATE NOCASE",zoneid);resultado = db_query(DataBaseDB,Query);
	if(!db_num_rows(resultado))
	{
		SendClientMessage(playerid,COLOR_ROJO,"Esta zona no existe.");
