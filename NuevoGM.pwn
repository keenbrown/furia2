							if(autospermisos[i][aps_clan] != PlayerData[playerid][cClanid])
							{
								ejectSpecialVehicle(playerid);
							}
						}
					}
				}
			}
		}
	}
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
 	{
// 		if(!esFundador(playerid))
