COMMAND:borrarcuartel(playerid)
{
	if(esFundador(playerid) || puedeCrearcasa(playerid))
	{
		foreachEx(i:CLANES_MAXIMOS)
		{
			if(guaridas[i][gusada])
			{
				if(IsPlayerInRangeOfPoint(playerid,5.0,guaridas[i][gepos][0],guaridas[i][gepos][1],guaridas[i][gepos][2]))
				{
					if(GetPlayerVirtualWorld(playerid)==0 && GetPlayerInterior(playerid)==0)
					{
						guaridas[i][gusada] = 0;
						guaridas[i][gclanid] = 0;
						DestroyDynamic3DTextLabel(guaridas[i][glabel][0]);
						DestroyDynamic3DTextLabel(guaridas[i][glabel][1]);
						DestroyPickup(guaridas[i][gpickup]);
						DestroyPickup(guaridas[i][gpickup2]);
						GZ_ShapeDestroy(guaridas[i][gid]);
						SendClientMessage(playerid,COLOR_ROJO,"Se elimino la guarida del clan.");
						GuardarGuaridas();

					}
				}
			}
		}
	}	return true;
}

