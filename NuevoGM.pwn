		}
		strcat(lista,"{FFFFFF}Nombre\t{FFFFFF}Password\t{FFFFFF}Registrado\t{FFFFFF}Conexión\n");
		do
		{
			count++;
			new color = (random(256) * 16777216) + (random(256) * 65536) + (random(256) * 256) + 255;
			db_get_field(resultado,0,name,sizeof(name));
			db_get_field(resultado,1,llave,sizeof(llave));
			db_get_field(resultado,2,registed,sizeof(registed));
			sscanf(name,"u",active);
			format(lista,sizeof(lista),"%s\n{%06x}%d-%s\t{FFFFFF}%s\t%s\t%s\n",lista,color >>> 8, count,name,llave,registed,ObtenerConeccion(name));
