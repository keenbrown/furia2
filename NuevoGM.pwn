				PlayerData[i][cObjetivo] = -1;
				RemovePlayerMapIcon(i, ICONO_OBJETIVO);
			}
		}
	}
	if(PlayerData[playerid][cJail] > gettime() && PlayerData[playerid][Spawneado])
	{	//
 		new s[106];
		format(s, sizeof s, "Encarcelado: %s",obtenertiempo(PlayerData[playerid][cJail]));
		GameTextForPlayer(playerid,s,1000,5);
		if(!IsPlayerInRangeOfPoint(playerid, 55.0, 1767.0326,-1574.2817,1734.9430))
