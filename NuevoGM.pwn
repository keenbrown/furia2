COMMAND:estacionar(playerid, params[])
{
	new vehicleid = GetOwnedVehicle(playerid), Float:pos[4],str[500];
	if(!IsPlayerInAnyVehicle(playerid) || vehicleid != GetOwnedVehicle(playerid)) return SendClientMessage(playerid, COLOR_ROJO, "Debe estar en un vehículo propio para usar este comando.");
    if(vehicleid == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_ROJO, "SERVIDOR: este vehiculo no es suyo.");
	GetVehiclePos(GetOwnedVehicle(playerid), pos[0], pos[1], pos[2]);
	GetVehicleZAngle(GetOwnedVehicle(playerid), pos[3]);
	format(str,sizeof(str),"UPDATE `VehPers` SET `X` = '%f', `Y` = '%f', `Z` = '%f', `A` = '%f' WHERE `DUENO` = '%s' COLLATE NOCASE",pos[0],pos[1],pos[2],pos[3],DB_Escape(Nombre(playerid)));
	db_query(DataBaseDB,str);
	GameTextForPlayer(playerid, "~g~estacionado", 3000, 5);
	return 1;
}

COMMAND:lock(playerid, params[])
{
    new vehicleid = GetOwnedVehicle(playerid), Float:pos[3], engine, lights, alarm, doors, bonnet, boot, condition;
    if(vehicleid == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_ROJO, "SERVIDOR: Debe tener un vehículo para usar este comando.");
	GetVehiclePos(vehicleid, pos[0], pos[1], pos[2]);
	if(!IsPlayerInRangeOfPoint(playerid, 10.0, pos[0], pos[1], pos[2])) return SendClientMessage(playerid, COLOR_ROJO, "SERVIDOR: debe estar al lado de su propio vehículo para usar este comando.");
	if(VehicleInfo[vehicleid][vehicle_locked] == 0)
	{
	    VehicleInfo[vehicleid][vehicle_locked] = 1;
		SetVehicleParamsForAll(vehicleid, 0, 1);
        GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, condition);
		SetVehicleParamsEx(vehicleid, engine, lights, alarm, 1, bonnet, boot, condition);
		GameTextForPlayer(playerid, "~r~Cerrado", 3000, 5);
		return 1;
	}
	else
	{
	    VehicleInfo[vehicleid][vehicle_locked] = 0;
		SetVehicleParamsForAll(vehicleid, 0, 0);
        GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, condition);
		SetVehicleParamsEx(vehicleid, engine, lights, alarm, 0, bonnet, boot, condition);
		GameTextForPlayer(playerid, "~g~Abierto", 3000, 5);
	}	return 1;
}
