			SetPlayerAttachedObject(i, 8, 19418, 5, -0.011000, 0.028000, -0.022000, -15.600012, -33.699977, -81.700035, 0.891999, 1.000000, 1.168000);
			SetPlayerAttachedObject(i, 9, 19418, 6, -0.011000, 0.028000, -0.022000, -15.600012, -33.699977, -81.700035, 0.891999, 1.000000, 1.168000);
			arrestado[i] = true;
			new ms[199];
