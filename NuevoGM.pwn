			format(IPInfo[playerid], MAX_PLAYER_NAME, "%s", ip);
		    format(str, sizeof(str), "www.ipqualityscore.com/api/ip_lookup.php?KEY=onekz5t6ksv8aidtrhcj7bhjit7u0&IP=%s", ip);
		}
