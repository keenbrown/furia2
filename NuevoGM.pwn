			SetVehicleParamsEx(AutosJobs[i][aJob_vehicle],0,0,0,0,0,0,0);
			SetVehicleParamsCarWindows(AutosJobs[i][aJob_vehicle], 1, 1, 1, 1);
			PutPlayerInVehicle(playerid, AutosJobs[i][aJob_vehicle], 0);
			//
			format(tmp, sizeof(tmp), "SELECT unico FROM AutosJob WHERE unico=%d", i);
			result = db_query(DataBaseDB, tmp);
			if(!db_num_rows(result))
			{
				format(tmp, sizeof(tmp), "INSERT INTO AutosJob(unico,uso,color0,color1,pos0,pos1,pos2,pos3,job,modelo)VALUES(%d,%d,%d,%d,%0.3f,%0.3f,%0.3f,%0.3f,%d,%d);",
