		}
	}
	else if(PlayerData[playerid][cJail] && gettime() > PlayerData[playerid][cJail])
	{
		PlayerData[playerid][cJail] = 0;
		SpawnPlayer(playerid);
		ShowInfo(playerid,"Condena completada.");
	}
	
	else if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
	{
	if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 539)
 		SetPlayerArmedWeapon(playerid, 0);
    }

	if(GetPlayerMoney(playerid) != PlayerData[playerid][Dinero])
	{
		if(AcMoney[playerid] > 2)
		{
			ShowPlayerDialog(playerid, DIALOGO_UNO, DIALOG_STYLE_MSGBOX,"{FFFFFF}Anticheat - "colorkaki"LifeCity","{ffffff}Has sido detectado automaticamente por el servidor\n"colorlima"[Razon]:{ffffff} Money Hack.","OK","");
			SendClientMessageEx(-1,COLOR_ROJO,"« Anticheat » {FFFFFF}%s Posible money hack su dinero fue reseteado.",Nombre(playerid));
			AcMoney[playerid]++;
		}
			ResetPlayerMoney(playerid);
			GivePlayerMoney(playerid,PlayerData[playerid][Dinero]);
		}//Evitar hack de Score
		if(GetPlayerScore(playerid) != PlayerData[playerid][Score])
		{
			SetPlayerScore(playerid,PlayerData[playerid][Score]);
		}//Checar si es usuario RCON permitido
		if(IsPlayerAdmin(playerid))
		{
			if(!esRcon(playerid) || !esFundador(playerid))
			{
				SendClientMessageEx(-1,COLOR_ROJO,"« Anticheat » {ffffff}%s fue expulsado por ingresar a la RCON sin autorizacion.",Nombre(playerid));
				Kick(playerid);
			}
		}// Contar fps
		new FPS;FPS = GetPlayerDrunkLevel(playerid);
		if (FPS < 100)
		{
			SetPlayerDrunkLevel(playerid, 2000);
		}
		else
		{
		if (pDrunkLevelLast[playerid] != FPS)
		{
			new wfps = pDrunkLevelLast[playerid] - FPS;
			if ((wfps > 0) && (wfps < 200))
			pFPS[playerid] = wfps;pDrunkLevelLast[playerid] = FPS;
		}
	}
	if(IsPlayerConnected(playerid))
	{
		new st[185];
		if(gettime() <= Zonas[Zona@Time@Restante[playerid]][Tiempo@restante@conquista])
		{
			format(st,sizeof(st),"~g~Tiempo restante de conquista~n~~w~%s",obtenertiempo(Zonas[Zona@Time@Restante[playerid]][Tiempo@restante@conquista]));
			PlayerTextDrawSetString(playerid,Zonas@Tiempo@Restante@Conquista,st);
		}
	}
	if(robandotienda[playerid] && trobandotienda[playerid] < gettime())
	{
		new BotinRandom;PlayerTextDrawHide(playerid,Zonas@Tiempo@Restante@Conquista);
		switch(random (5))
		{
			case 0:	{BotinRandom = random(5000);Earn(playerid, BotinRandom);SendClientMessageEx(playerid,-1,"{A2F72B} {ffffff}Robaste $%d {FF0000}huye con el botin",BotinRandom);robandotienda[playerid] = false; trobandotienda[playerid] = 0;}
