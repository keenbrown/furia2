				AutosJobs[i][aJob_posicion][0], AutosJobs[i][aJob_posicion][1], AutosJobs[i][aJob_posicion][2], AutosJobs[i][aJob_posicion][3],
				AutosJobs[i][aJob_job], AutosJobs[i][aJob_modelo]);
				db_free_result(db_query(DataBaseDB, tmp));
			}
			else
			{
				format(tmp, sizeof(tmp), "UPDATE AutosJob SET uso=%d,color0=%d,color1=%d,pos0=%0.3f,pos1=%0.3f,pos2=%0.3f,pos3=%0.3f,job=%d,modelo=%d WHERE unico=%d;",
				AutosJobs[i][aJob_uso], AutosJobs[i][aJob_color][0], AutosJobs[i][aJob_color][1],
				AutosJobs[i][aJob_posicion][0], AutosJobs[i][aJob_posicion][1], AutosJobs[i][aJob_posicion][2], AutosJobs[i][aJob_posicion][3],
				AutosJobs[i][aJob_job], AutosJobs[i][aJob_modelo], i);
				db_free_result(db_query(DataBaseDB, tmp));
