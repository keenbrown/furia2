COMMAND:crearauto(playerid, args[])
{
	if(!esFundador(playerid))return ShowError(playerid, "Sin permisos para usar este comando.");
	new model, zonaid, tmp[164], DBResult:result, col01, col02;
	if(sscanf(args, "dddd",zonaid, model, col01, col02))return ShowError(playerid, "Use: /crearauto [Auto Clan ID][Auto Modelo ID][Color 1][Color 2]");
	if(!(400 <= model <= 611))return MensajeError(playerid, "El ID no es valido.");
	if(!(0 <= col01 <= 255))return ShowError(playerid, "Solo existen 255 colores.");
	if(!(0 <= col02 <= 255))return ShowError(playerid, "Solo existen 255 colores.");
	for(new i=0;i<sizeof(autospermisos);i++){
		if(!autospermisos[i][aps_uso])
		{
			autospermisos[i][aps_uso] = 1;
			autospermisos[i][aps_clan] = zonaid;
//			autospermisos[i][aps_zona] = zonaid;
			autospermisos[i][aps_modelo] = model;
			autospermisos[i][aps_color][0] = col01;
			autospermisos[i][aps_color][1] = col02;
			GetPlayerPos(playerid, autospermisos[i][aps_posicion][0], autospermisos[i][aps_posicion][1], autospermisos[i][aps_posicion][2]);
			GetPlayerFacingAngle(playerid, autospermisos[i][aps_posicion][3]);
