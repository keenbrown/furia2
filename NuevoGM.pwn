stock CargarMantas()
{
	foreachEx(d:MANTAS_MAX)mantas[d][m@usado]=0;
	new DBResult:R = db_query(DataBaseDB,"SELECT * FROM mantas");
	if(db_num_rows(R))
	{
		do
		{
			new i = db_get_field_assoc_int(R,"id");
			mantas[i][m@id] = i;
			mantas[i][m@usado] = db_get_field_assoc_int(R,"usado");
			db_get_field_assoc(R,"texto",mantas[i][m@texto],42);
			db_get_field_assoc(R,"dueno",mantas[i][m@dueno],25);
			mantas[i][m@x] = db_get_field_assoc_float(R,"x");
			mantas[i][m@y] = db_get_field_assoc_float(R,"y");
			mantas[i][m@z] = db_get_field_assoc_float(R,"z");
			mantas[i][m@rx] = db_get_field_assoc_float(R,"rx");
			mantas[i][m@ry] = db_get_field_assoc_float(R,"ry");
			mantas[i][m@rz] = db_get_field_assoc_float(R,"rz");
			new sq[126];
			format(sq, sizeof sq, "%s", mantas[i][m@texto]);
