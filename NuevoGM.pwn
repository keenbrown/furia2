stock CambiarUserName(playerid,newname[])
{
   	new query[130],DBResult:resultado;
  	format(query,sizeof(query),"SELECT `ID` FROM `Usuarios` WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(newname));
   	resultado = db_query(DataBaseDB,query);
   	if(db_num_rows(resultado))
   	{
   		SendClientMessage(playerid,COLOR_ROJO,"Ese nombre ya se encuentra registrado.") && ShowPlayerDialog(playerid,DIALOGO_CAMBIAR_NOMBRE,DIALOG_STYLE_INPUT,"{FFFFFF}Cambio de nombre - "colorkaki"LifeCity","{FFFFFF}Escribe el nombre que deseas tener.","Cambiar","Cancelar")&&db_free_result(resultado);
   	}
	else
	{
	   	db_query(DataBaseDB,"BEGIN");
	   	format(query,sizeof(query),"UPDATE `Usuarios` SET Nombre = '%s' WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(newname),Nombre(playerid));
	   	db_query(DataBaseDB,query);
	   	db_query(DataBaseDB,"COMMIT");

	   	format(query,sizeof(query),"SELECT `ID` FROM `Clanmiembros` WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(Nombre(playerid)));
	   	resultado = db_query(DataBaseDB,query);
	   	if(db_num_rows(resultado))
		{
		   	db_query(DataBaseDB,"BEGIN");
		   	format(query,sizeof(query),"UPDATE `Clanmiembros` SET `Nombre` = '%s' WHERE `Nombre` = '%s' COLLATE NOCASE",newname,DB_Escape(Nombre(playerid)));
		   	db_query(DataBaseDB,query);
		   	db_query(DataBaseDB,"COMMIT");
		   	if(Clan[playerid][Creador] == 1)
			{
			   	format(Clanes[Clan[playerid][Miembro]][aLider],28,"%s",newname);
			   	db_query(DataBaseDB,"BEGIN");
			   	format(query,sizeof(query),"UPDATE `Clanes` SET `Lider` = '%s' WHERE `ID` = '%d' COLLATE NOCASE",newname,Clan[playerid][Miembro]);
			   	db_query(DataBaseDB,query);
			   	db_query(DataBaseDB,"COMMIT");
	  		}
	   	}db_free_result(resultado);
	   	format(query,sizeof(query),"SELECT `ID` FROM `Banco` WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(Nombre(playerid)));
	   	resultado = db_query(DataBaseDB,query);
	   	if(db_num_rows(resultado))
		{
		   	db_query(DataBaseDB,"BEGIN");
		   	format(query,sizeof(query),"UPDATE `Banco` SET `Nombre` = '%s' WHERE `Nombre` = '%s' COLLATE NOCASE",newname,DB_Escape(Nombre(playerid)));
		   	db_query(DataBaseDB,query);
		   	db_query(DataBaseDB,"COMMIT");
 		}db_free_result(resultado);
		format(query,sizeof(query),"SELECT `ID` FROM `Casas` WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(Nombre(playerid)));
		resultado = db_query(DataBaseDB,query);
		if(db_num_rows(resultado))
  		{
	   		do
  			{
		   		format(query,sizeof(query),"UPDATE `Casas` SET `Dueno` = '%s' WHERE `Dueno` COLLATE NOCASE",newname,DB_Escape(Nombre(playerid)));
		   		db_query(DataBaseDB,query);
	   		}while(db_next_row(resultado));
	   	}db_free_result(resultado);
	   	SendClientMessageEx(-1,COLOR_AMARILLO,"* %s se ha cambiado el nombre a: [%s]",Nombre(playerid),newname);
	   	new nNick[24];
	   	format(nNick,sizeof(nNick),"%s",newname);
	   	SetPlayerName(playerid,nNick);
	   	PlayerData[playerid][Coins] -= 1;
	}
}

