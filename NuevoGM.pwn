	}
	if(PlayerData[playerid][uHoras] >= 24)
	{
		PlayerData[playerid][uDias] += 1;
		PlayerData[playerid][uHoras] -= 24;
	}
	if(PlayerData[playerid][uDias] >= 7)
	{
		PlayerData[playerid][uSemanas] += 1;
		PlayerData[playerid][uDias] -= 7;
	}
	if(PlayerData[playerid][uSemanas] >= 4)
	{
		PlayerData[playerid][uMeses] += 1;
		PlayerData[playerid][uSemanas] -= 4;
	}
	return 1;
}
