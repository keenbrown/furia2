deleteVehicle(vehicleid)
{
	for(new i=0;i<MAX_PLAYERS;i++)
	{
		if(PlayerData[i][cCoche] == vehicleid)
		{
			PlayerData[i][cCoche] = -1;
			DestroyVehicle(vehicleid);
			return true;
		}
	}
	return false;
}

