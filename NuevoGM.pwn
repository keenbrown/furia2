public OnPlayerEnterRaceCheckpoint(playerid)
{
	if(EnCarguero[playerid] == 0)
 	{
		if(EnCarrera == 0)
		{
			DisablePlayerRaceCheckpoint(playerid);
			StopGPSPepe(playerid);
			ShowInfo(playerid, "GPS: Ha llegado a su destino.");
		}
	}
	if(EnCarguero[playerid] == 1)
	{
		if(EnCarrera == 1)
		{
			DisablePlayerRaceCheckpoint(playerid);
			GameTextForPlayer(playerid, "~w~Busca la Palanca\n\n~g~y carga el barco", 10000, 3);
			EnCarrera = 2;
			new vehicleid = GetPlayerVehicleID(playerid);
			SetVehiclePos(vehicleid, 3747.2642,-2073.9258,0.7968);
	     	SetVehicleZAngle(vehicleid, 269.3547);
	     	SetPlayerRaceCheckpoint(playerid, 2, 3701.4253,-2016.4569,6.0919, 0.0,0.0,0.0, 3.0);
		}
		else if(EnCarrera == 4)
		{
			new vehicleid = GetPlayerVehicleID(playerid);
			SetVehiclePos(vehicleid, 2653.9685,-2268.3972,0.0083);
	     	SetVehicleZAngle(vehicleid, 268.8070);
	     	GameTextForPlayer(playerid, "~n~~y~~h~Desargando Petroleo...", 20000, 3);
			SetTimerEx("TiempodesCarga",20000,false,"d",playerid);
			TogglePlayerControllable(playerid, 0);
		}
	}
	else if(EnCarrera == 2)
	{
		TogglePlayerControllable(playerid, 0);
	    GameTextForPlayer(playerid, "~n~~y~~h~Cargando Petroleo...", 30000, 3);
		SetTimerEx("TiempodeCarga",30000,false,"d",playerid);
		DisablePlayerRaceCheckpoint(playerid);
		EnCarrera= 3;
	}
    return 1;
}

