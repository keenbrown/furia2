EvitarCrashAnuncio(string[])
{//evitar crash del comando /anuncio
	new evitarcrash[100];
	format(evitarcrash,sizeof(evitarcrash),"%s",string);
	new evitarcrashh = strlen(string);
	for(new i=0;i<evitarcrashh;i++)
	{
		if(evitarcrash[i] == '$')
		{
			evitarcrash[i] = '%';
		}
	}
	return evitarcrash;
}

