COMMAND:pasarcoins(playerid,params[])
{
	new jugador,cantidad;
	if(sscanf(params,"ud",jugador,cantidad))return ShowError(playerid,"Usa: /pasarcoins [Nombre - Jugador] [Cantidad]");
	if(!IsPlayerConnected(jugador))return SendClientMessage(playerid,COLOR_ROJO,"El jugador no se encuentra conectado.");
	if(playerid == jugador)return SendClientMessage(playerid,COLOR_ROJO,"No puedes seleccionarte a ti mismo.");
	if(gettime() <= ChecarComandoVIP[playerid][9])return SendClientMessageEx(playerid,COLOR_ROJO,"Debes esperar %d segundos para usar de nuevo este comando.",ChecarComandoVIP[playerid][9]-gettime());
	if(cantidad >= 1 && cantidad <= 100)
	{
		if(PlayerData[playerid][Coins] >= cantidad)
		{
			PlayerData[playerid][Coins] -= cantidad;
			PlayerData[jugador][Coins] += cantidad;
			ChecarComandoVIP[playerid][9] = gettime() + 60*4;
			SendClientMessageEx(playerid,COLOR_KAKI,"Le has transpasado [%d] %s ah %s.",cantidad,macro(cantidad==1,"Coin","Coins"),Nombre(jugador));
			SendClientMessageEx(jugador,COLOR_KAKI,"%s te ah transpasado [%d] %s.",Nombre(playerid),cantidad,macro(cantidad==1,"Coin","Coins"));
		}else return SendClientMessageEx(playerid,COLOR_ROJO,"No tienes suficientes coins | tienes[%d] | intentastes dar[%d]",PlayerData[playerid][Coins],cantidad);
	}else return SendClientMessage(playerid,COLOR_ROJO,"Minimo [1] coin - Maximo [100] coins.");
	return 1;
}

