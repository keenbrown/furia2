	  		if (vacaInfo[i][vacaObject] && !vacaInfo[i][vacaDead])
			{
			    static Float:pos[3];
			   	getVacaPosition(i, pos[0], pos[1], pos[2]);// Pegar a posição do player
			    vacaInfo[i][vacaHealth] -= Damage;
	            if (!vacaInfo[i][vacaAssustado]) vacaInfo[i][vacaAssustado] = true;
