COMMAND:darlider(playerid,params[])
{
	if(esRcon(playerid) || esFundador(playerid))
	{
		new Jugador,clanid,ccreador;
		if(sscanf(params,"udd",Jugador,clanid,ccreador))return ShowError(playerid,"Usa: /darlider [Nombre - ID] [clanid] [Creador: 0=no - 1=si]");
		if(!IsPlayerConnected(Jugador))return SendClientMessage(playerid,COLOR_ROJO,"El jugador no se encuentra conectado.");
		new Query[456],DBResult:resultado;
		if(Clanes[clanid][aSlot]==0)return SendClientMessage(playerid,COLOR_ROJO,"Clan no registrado.");
		if(ccreador == 0 || ccreador == 1){
		format(Query,sizeof(Query),"SELECT * FROM `Clanmiembros` WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(Nombre(Jugador)));
		resultado = db_query(DataBaseDB,Query);
		if(db_num_rows(resultado))
		{
			db_query(DataBaseDB,"BEGIN");
			format(Query,sizeof(Query),"UPDATE `Clanmiembros` SET `Miembro` = '%d',`Creador` = '%d',`Lider` = '1',`Rango` = '7' WHERE `Nombre` = '%s' COLLATE NOCASE",clanid,ccreador,DB_Escape(Nombre(Jugador)));
			db_query(DataBaseDB,Query);
			db_query(DataBaseDB,"COMMIT");
			Clanes[Clan[Jugador][Miembro]][aMiembros] --;
			if(Clan[Jugador][Miembro] == clanid)
			{
				Clanes[clanid][aMiembros] ++;
			}
			else
			{
				Clanes[clanid][aMiembros] ++;
			}
			Clan[Jugador][Miembro] = clanid;
			Clan[Jugador][Creador] = ccreador;
			Clan[Jugador][Lider]   = 1;
			Clan[Jugador][Rango]   = 7;
			PlayerData[Jugador][cClanid] = clanid;
			if(Clanes[clanid][aAntiTeam] == 1)
			{
				SetPlayerTeam(Jugador,clanid);
			}
//			SetPlayerColor(Jugador,coloresclanlista[Clanes[clanid][aColor]]);
			SetPlayerSkin(Jugador,Clanes[clanid][aSkin]);
			SendClientMessageEx(Jugador,COLOR_AMARILLO,"* El administrador %s te otorgo el clan %s",Nombre(playerid),Clanes[clanid][aNombre]);
			SendClientMessageEx(playerid,COLOR_AZULCLARO,"Le otorgastes el clan %s a %s",Clanes[clanid][aNombre],Nombre(Jugador));
		}
		else
		{
			format(Query,sizeof(Query),"INSERT INTO `Clanmiembros` (`Nombre`,`Miembro`,`Creador`,`Lider`,`Rango`) VALUES ('%s','%d','%d','1','7')",DB_Escape(Nombre(Jugador)),clanid,ccreador);
			db_query(DataBaseDB,Query);
			Clan[Jugador][Miembro] = clanid;
			Clan[Jugador][Creador] = ccreador;
			Clan[Jugador][Lider]   = 1;
			Clan[Jugador][Rango]   = 7;
			Clanes[clanid][aMiembros] ++;
			PlayerData[Jugador][cClanid] = clanid;
			if(Clanes[clanid][aAntiTeam] == 1)
			{
				SetPlayerTeam(Jugador,clanid);
			}
//			SetPlayerColor(Jugador,coloresclanlista[Clanes[clanid][aColor]]);
			SetPlayerSkin(Jugador,Clanes[clanid][aSkin]);
			SendClientMessageEx(Jugador,COLOR_AMARILLO,"* El administrador %s te otorgo el clan %s",Nombre(playerid),Clanes[clanid][aNombre]);
			SendClientMessageEx(playerid,COLOR_AZULCLARO,"Le otorgastes el clan %s a %s",Clanes[clanid][aNombre],Nombre(Jugador));
			}db_free_result(resultado);
			if(ccreador == 1)
			{
				format(Clanes[clanid][aLider],32,"%s",Nombre(Jugador));
				new quer[116];
				db_query(DataBaseDB,"BEGIN");
				format(quer,sizeof(quer),"UPDATE `Clanes` SET `Lider` = '%s' WHERE `ID` = '%d' COLLATE NOCASE",Nombre(Jugador),clanid);db_query(DataBaseDB,quer);
				db_query(DataBaseDB,"COMMIT");
			}
			adminRegistros(playerid,"darlider");
		}else return SendClientMessage(playerid,COLOR_ROJO,"Uso de creador: 0=no - 1=si");
	}else return SendClientMessage(playerid,COLOR_ROJO,"No tienes permisos para usar este comando.");
	return 1;
}

COMMAND:reiniciarclan(playerid,params[])
{
	if(esRcon(playerid) || esFundador(playerid))
	{
		new clanid,Query[286];
		if(sscanf(params,"d",clanid))return ShowError(playerid,"Usa: /reiniciarclan [clanid]");
		if(Clanes[clanid][aSlot] == 0)return SendClientMessage(playerid,COLOR_ROJO,"Clan no registrado.");
		SendClientMessageEx(-1,COLOR_ROJO,"* El Administrador %s reinicio el clan "colorblanco"%s",Nombre(playerid),Clanes[clanid][aNombre]);
		new DBResult:zresult = db_query(DataBaseDB,"SELECT `ID` FROM `Zonas` WHERE `ID` > 0");
		if(db_num_rows(zresult))
		{
			do
				{
					new z = db_get_field_int(zresult,0);
					if(z == clanid)
				{
					Zonas[z][Dueno] = 0;
				}
			}while(db_next_row(zresult));
		}db_free_result(zresult);
		foreach(new i : Player)
		{
			if(IsPlayerConnected(i))
			{
				if(Clan[i][Miembro] == clanid)
				{
					Clan[i][Miembro] = 0;
					Clan[i][Creador] = 0;
					Clan[i][Lider] = 0;
					Clan[i][Rango] = 0;
					SendClientMessageEx(i,COLOR_KAKI,"Su clan fue reiniciado por el administrador "colorblanco"[%s]",Nombre(playerid));
				}
			}
		}
		format(Clanes[clanid][aNombre],12,"Server");
		format(Clanes[clanid][aLider],12,"Server");
		format(Clanes[clanid][aTag],12,"Server");
		format(Clanes[clanid][aCreado],12,"Server");
		Clanes[clanid][aSlot] = 1;
		Clanes[clanid][aMiembros] = 0;
		Clanes[clanid][aConquista] = 0;
		Clanes[clanid][aSkin] = 23;
		Clanes[clanid][aColor] = 23;
		Clanes[clanid][aAntiTeam] = 0;
		Clanes[clanid][aKills] = 0;
		Clanes[clanid][aDeaths] = 0;
		Clanes[clanid][aNivel] = 0;
		Clanes[clanid][clClase] = 0;
		format(Query,sizeof(Query),"DELETE FROM `Clanmiembros` WHERE `Miembro` = '%d' COLLATE NOCASE",clanid);
		db_query(DataBaseDB,Query);
		adminRegistros(playerid,"REINICIARCLAN");
	}else return SendClientMessage(playerid,COLOR_ROJO,"No tienes permisos para usar este comando.");
	return 1;
}

