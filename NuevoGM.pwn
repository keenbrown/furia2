stock ObtenerMiembrosEnZona(zoneid,equipo)//Funcion para saber que cantidad de miembros hay en la zona.
{
	new count = 0;
	new Float:px, Float:py, Float:pz;
	for(new i=0,m=GetMaxPlayers();i<m;i++)
	{
		if(IsPlayerConnected(i))
		{
			if(GetPlayerState(i) != PLAYER_STATE_WASTED && Clan[i][Miembro] == equipo)
			{
				GetPlayerPos(i, px, py, pz);
				if(px > Zonas[zoneid][zPos][0] - ZONA_LONGITUD && py > Zonas[zoneid][zPos][1] - ZONA_LONGITUD && px < Zonas[zoneid][zPos][0] + ZONA_LONGITUD && py < Zonas[zoneid][zPos][1] + ZONA_LONGITUD)
				{
					count++;
				}
			}
		}
	}	return count;
}

