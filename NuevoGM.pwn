COMMAND:clearmanta(playerid)
{
	if(esFundador(playerid))
	{
		foreachEx(i:MANTAS_MAX)
		{
			if(mantas[i][m@usado])
			{
				if(IsPlayerInRangeOfPoint(playerid,5.0,mantas[i][m@x],mantas[i][m@y],mantas[i][m@z]))
