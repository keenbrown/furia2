	        case 4: {SendClientMessageEx(playerid,-1,"{A2F72B} {ffffff}El Vendedor decide {FF0000}no darte Nada, {ffffff}vete");trobandotienda[playerid] = 0;}
		}
	}
	if(robandotienda[playerid] && trobandotienda[playerid] > gettime())
	{
		new tmp[56];
		if(gettime() - trobandotiendaporcentaje[playerid])
		{
			PlayerTextDrawShow(playerid,Zonas@Tiempo@Restante@Conquista);
			format(tmp, sizeof tmp, "~r~Robando tienda: ~y~%0.2f%%",( ( (gettime()-trobandotiendaporcentaje[playerid]) /1.1) ) );
			PlayerTextDrawSetString(playerid,Zonas@Tiempo@Restante@Conquista,tmp);
		}
	}	//--|Sistema de autos|--->
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
 	{
 		if(!esFundador(playerid))
	 	{
			new vehicleid;
			for(new i=0; i<sizeof(autospermisos); i++)
			{
				if(IsPlayerInAnyVehicle(playerid))
				{
					vehicleid = GetPlayerVehicleID(playerid);
					if(autospermisos[i][aps_vehicle] == vehicleid)
					{
						if(!PlayerData[playerid][cClanid] || !autospermisos[i][aps_clan])
						{
							ejectSpecialVehicle(playerid);
						}
						else if(autospermisos[i][aps_clan])
