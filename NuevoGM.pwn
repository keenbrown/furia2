	format(Query,sizeof(Query),"DELETE FROM `Zonas` WHERE `ID` = '%d' COLLATE NOCASE",zoneid);db_query(DataBaseDB,Query);
	GangZoneDestroy(Zonas[zoneid][ID]);
	Area_Delete(RectangleZonas[zoneid]);
