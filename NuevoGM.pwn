	for(new vaca; vaca != MAX_VACAS; vaca++)
	{
	    new rdon = random(sizeof VacaSpawns);
	    vacaCreate(VacaSpawns[rdon][0]+frandom(100.0)-frandom(100.0), VacaSpawns[rdon][1]+frandom(100.0)-frandom(100.0));
	}
}

stock vacaGetGroundZ(Float:x, Float:y, &Float:z)
{
	CA_FindZ_For2DCoord(x, y, z);
}

stock vacaCreate(Float:x, Float:y, Float:z=0.0)
{
	vacaGetGroundZ(x, y, z);
	vacaInfo[countVacas][vacaDead]      = false;
	vacaInfo[countVacas][vacaAssustado] = false;
    vacaInfo[countVacas][vacaRespawn]   = 0;
    vacaInfo[countVacas][vacaHealth]    = 30.0;
	vacaInfo[countVacas][vacaObject] = CreateDynamicObject(19833, x, y, z, 0.0, 0.0, random(360), 0);
    countVacas++;
	return 0;
}

stock vacaMove(vacaid, Float:x, Float:y, Float:z)
{
	if (!vacaInfo[vacaid][vacaDead])
	{
	    new Float:px, Float:py, Float:pz;
	    getVacaPosition(vacaid, px, py, pz);
	    vacaGetGroundZ(x, y, z);
	  //SetDynamicObjectRot(deerInfo[deerid][deerObject], 0.0, 0.0, (GetPointAngleToPoint(px, py, x, y) + 90.0));
	    SetDynamicObjectRot(vacaInfo[vacaid][vacaObject], 0.0, 0.0, (GetPointAngleToPoint(px, py, x, y) + 180.0));
	   	MoveDynamicObject(vacaInfo[vacaid][vacaObject], x, y, z - 0.10, vacaInfo[vacaid][vacaAssustado] ? (VACA_SPEED_ASSUSTADO) : (VACA_SPEED_NORMAL));
	    return 1;
	}return 0;
}

