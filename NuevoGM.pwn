public OnPlayerEditDynamicObject(playerid, objectid, response, Float:x, Float:y, Float:z, Float:rx, Float:ry, Float:rz)
{
	new f_id;
	f_id = Iter_Free(Foods);
	if(EditingTreeID[playerid] != -1 && Iter_Contains(Trees, EditingTreeID[playerid]))
	{
	    if(response == EDIT_RESPONSE_FINAL)
	    {
	        new id = EditingTreeID[playerid];
	        TreeData[id][treeX] = x;
	        TreeData[id][treeY] = y;
	        TreeData[id][treeZ] = z;
	        TreeData[id][treeRX] = rx;
	        TreeData[id][treeRY] = ry;
	        TreeData[id][treeRZ] = rz;

	        SetDynamicObjectPos(objectid, TreeData[id][treeX], TreeData[id][treeY], TreeData[id][treeZ]);
	        SetDynamicObjectRot(objectid, TreeData[id][treeRX], TreeData[id][treeRY], TreeData[id][treeRZ]);

			Streamer_SetFloatData(STREAMER_TYPE_3D_TEXT_LABEL, TreeData[id][treeLabel], E_STREAMER_X, TreeData[id][treeX]);
			Streamer_SetFloatData(STREAMER_TYPE_3D_TEXT_LABEL, TreeData[id][treeLabel], E_STREAMER_Y, TreeData[id][treeY]);
			Streamer_SetFloatData(STREAMER_TYPE_3D_TEXT_LABEL, TreeData[id][treeLabel], E_STREAMER_Z, TreeData[id][treeZ] + 1.5);

		    stmt_bind_value(UpdateTree, 0, DB::TYPE_FLOAT, TreeData[id][treeX]);
		    stmt_bind_value(UpdateTree, 1, DB::TYPE_FLOAT, TreeData[id][treeY]);
		    stmt_bind_value(UpdateTree, 2, DB::TYPE_FLOAT, TreeData[id][treeZ]);
		    stmt_bind_value(UpdateTree, 3, DB::TYPE_FLOAT, TreeData[id][treeRX]);
		    stmt_bind_value(UpdateTree, 4, DB::TYPE_FLOAT, TreeData[id][treeRY]);
		    stmt_bind_value(UpdateTree, 5, DB::TYPE_FLOAT, TreeData[id][treeRZ]);
		    stmt_bind_value(UpdateTree, 6, DB::TYPE_INTEGER, id);
			stmt_execute(UpdateTree);

	        EditingTreeID[playerid] = -1;
	    }
	    if(response == EDIT_RESPONSE_CANCEL)
	    {
	        new id = EditingTreeID[playerid];
	        SetDynamicObjectPos(objectid, TreeData[id][treeX], TreeData[id][treeY], TreeData[id][treeZ]);
	        SetDynamicObjectRot(objectid, TreeData[id][treeRX], TreeData[id][treeRY], TreeData[id][treeRZ]);
	        EditingTreeID[playerid] = -1;
	    }
	}
	if(response == EDIT_RESPONSE_UPDATE)
 	{
 		if(objectid == FoodInfo[f_id] [IdFood])
		{
	        SetDynamicObjectPos(objectid, x, y, z);
	        SetDynamicObjectRot(objectid, rx, ry, rz);// to update it so others may see it.
	    }
 	}
	if (response == EDIT_RESPONSE_FINAL)
	{
		foreachEx(i:MANTAS_MAX)
		{
			if(mantas[i][m@usado])
			{
				if(mantas[i][m@manta] == objectid)
				{
					mantas[i][m@x] = x;
					mantas[i][m@y] = y;
					mantas[i][m@z] = z;
					mantas[i][m@rx] = rx;
					mantas[i][m@ry] = ry;
					mantas[i][m@rz] = rz;
					DestroyDynamicObject(mantas[i][m@manta]);
					mantas[i][m@manta] = CreateDynamicObject(19329,mantas[i][m@x],mantas[i][m@y],mantas[i][m@z],mantas[i][m@rx],mantas[i][m@ry],mantas[i][m@rz]);
					SetDynamicObjectMaterialText(mantas[i][m@manta], 0,mantas[i][m@texto], 130, "Arial", 28, 0, 0xFFFFFFFF, -16777216, 1);
					GuardarMantas(i);
					SendClientMessage(playerid,-1,"Se actualizo la posicion de la manta.");
				}
			}
		}
		foreachEx(i:MAX_ATM)
		{
			if(CajeroATM[i][c@usado])
			{
				if(CajeroATM[i][c@ATM] == objectid)
				{
					CajeroATM[i][c@x] = x;
					CajeroATM[i][c@y] = y;
					CajeroATM[i][c@z] = z;
					CajeroATM[i][c@rx] = rx;
					CajeroATM[i][c@ry] = ry;
					CajeroATM[i][c@rz] = rz;
					DestroyDynamicObject(CajeroATM[i][c@ATM]);// rota
					CajeroATM[i][c@label] = CreateDynamic3DTextLabel("{FFFFCC}/cajero.", -1, CajeroATM[i][c@x], CajeroATM[i][c@y], CajeroATM[i][c@z]+2, 150.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, 0, -1, -1, 50.0);
					CajeroATM[i][c@ATM] = CreateDynamicObject(19324,CajeroATM[i][c@x],CajeroATM[i][c@y],CajeroATM[i][c@z],CajeroATM[i][c@rx],CajeroATM[i][c@ry],CajeroATM[i][c@rz]);
					GuardarCajero(i);
					SendClientMessage(playerid,-1,"Se actualizo la posicion del Cajero.");
				}
			}
		}
		if(objectid == FoodInfo[f_id] [IdFood])
		{
			FoodInfo[f_id] [IdFood]= objectid;
			FoodInfo[f_id] [ObjPosFoodX] = x;
			FoodInfo[f_id] [ObjPosFoodY] = y;
			FoodInfo[f_id] [ObjPosFoodZ] = z;
			FoodInfo[f_id] [ObjRotFoodX] = rx;
			FoodInfo[f_id] [ObjRotFoodY] = ry;
			FoodInfo[f_id] [ObjRotFoodZ] = rz;
			SetDynamicObjectPos(objectid, x, y, z);
			SetDynamicObjectRot(objectid, rx, ry, rz);
		    new	skins[6];
			skins [0] = 171, skins[1] = 172, skins[2] = 169, skins[3] = 167, skins[4] = 168, skins[5] = 161;
		    ShowModelSelectionMenuEx(playerid, skins, 6, "Seleccionar skin de actor", DIALOGO_FOOD1, 16.0, 0.0, -55.0, 1.0, 0x00000033, 0x00000066, 0xA1A1A133);
		}
		else if(response == EDIT_RESPONSE_CANCEL)
		{
			print("no responde jajajajaja"); DestroyDynamicObject(FoodInfo[f_id] [IdFood]);
	//	}
	  	}
		new Merc_ID;
		Merc_ID = Iter_Free(MERCADOs);
		if(objectid == MercadoInfo[Merc_ID] [IDMercado])
		{
		    if(response == EDIT_RESPONSE_UPDATE)
		    {
		        SetDynamicObjectPos(objectid, x, y, z);
		        SetDynamicObjectRot(objectid, rx, ry, rz);// to update it so others may see it.
		    }
			if (response == EDIT_RESPONSE_FINAL)
			{
		 				MercadoInfo[Merc_ID] [IDMercado]= objectid;
						MercadoInfo[Merc_ID] [PosMercadoX] = x;
						MercadoInfo[Merc_ID] [PosMercadoY] = y;
						MercadoInfo[Merc_ID] [PosMercadoZ] = z;
						MercadoInfo[Merc_ID] [ObjRotMERCADOX] = rx;
						MercadoInfo[Merc_ID] [ObjRotMERCADOY] = ry;
						MercadoInfo[Merc_ID] [ObjRotMERCADOZ] = rz;
						SetDynamicObjectPos(objectid, x, y, z);
						SetDynamicObjectRot(objectid, rx, ry, rz);
					    new	skins[6];
						skins [0] = 171, skins[1] = 172, skins[2] = 169, skins[3] = 167, skins[4] = 168, skins[5] = 161;
					    ShowModelSelectionMenuEx(playerid, skins, 6, "Seleccionar skin de actor", DIALOGO_MERCADO1, 16.0, 0.0, -55.0, 1.0, 0x00000033, 0x00000066, 0xA1A1A133);
			}
			else if(response == EDIT_RESPONSE_CANCEL) { print("no responde jajajajaja"); DestroyDynamicObject(MercadoInfo[Merc_ID] [IDMercado]);
			}
		}
	}return true;
}

