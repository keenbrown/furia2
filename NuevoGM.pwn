COMMAND:controlusuarios(playerid,params[])
{// /controlusuarios
	if(esFundador(playerid))
	{
		Fixeo@OFFSET[playerid] = 0;
		new count = Fixeo@OFFSET[playerid];
		new name[24],registed[36],llave[36],lista[1600],active;
		new DBResult:resultado = db_query(DataBaseDB,"SELECT `Nombre`,`Llave`,`Registrado` FROM `Usuarios` WHERE `ID` > 0 LIMIT 20");
