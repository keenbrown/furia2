COMMAND:crearentrada(playerid,params[])
{
	if(esFundador(playerid) || puedeCrearcasa(playerid))
	{
		new namanya[65],sigps;
		foreachEx(i:MAX_PORT)
		{
			if(Portales[i][p@usado] == 0)
			{
				if(EnEntrada[playerid] == 0)
				{
					if(sscanf(params,"s[50]d", namanya,sigps)) return SendClientMessage(playerid, -1, "USAGE: /crearentrada <Name> <GPS>");
					if(sigps >=0 && sigps <= 1)
					{
						new Float:pos[3];
						GetPlayerPos(playerid,pos[0],pos[1],pos[2]);

						SetPVarInt(playerid, "vis", sigps);
		    			SetPVarString(playerid, "msg", namanya);
						SetPVarFloat(playerid, "Xpos", pos[0]);
						SetPVarFloat(playerid, "Ypos", pos[1]);
						SetPVarFloat(playerid, "Zpos", pos[2]);
						EnEntrada[playerid] = 1;
						return true;
					}else return SendClientMessage(playerid,-1,"use 0 No Gps / 1 Si GPS");
				}
				if(EnEntrada[playerid] == 1)
				{
					new Float:pos[3], Int, VW, Float:x, Float:y, Float:z, pmsg[65], gps, string[65];
