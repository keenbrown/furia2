GenerarAuto(playerid, modelo, Float:vida, colora=-1, colorb=-1)
{
	if(IsPlayerInAnyVehicle(playerid))
	{
		ShowError(playerid, "Usted ya esta dentro de un vehiculo.");
		return 0;
	}
	else
	{
		new Float:x, Float:y, Float:z, Float:angle;
		GetPlayerPos(playerid, x, y, z);
		GetPlayerFacingAngle(playerid, angle);
		if(PlayerData[playerid][cCoche] != -1)
		{
			deleteVehicle(PlayerData[playerid][cCoche]);
		}
		PlayerData[playerid][cCoche] = CreateVehicle(modelo, x, y+3, z+2, angle, -1, -1, -1);
		if(colora != -1){
			ChangeVehicleColor(PlayerData[playerid][cCoche], colora, colorb);
		}
		SetVehicleHealth(PlayerData[playerid][cCoche], vida);
		SetVehicleVirtualWorld(PlayerData[playerid][cCoche], GetPlayerVirtualWorld(playerid));
		LinkVehicleToInterior(PlayerData[playerid][cCoche], GetPlayerInterior(playerid));
//		PutPlayerInVehicle(playerid, PlayerData[playerid][cCoche], 0);
		SetVehicleParamsEx(PlayerData[playerid][cCoche],0,0,0,0,0,0,0);
		return true;
	}
}
