    Bought(playerid, CSAW_PRICE);
    GivePlayerWeapon(playerid, WEAPON_CHAINSAW, 1);
    new string[64];
    format(string, sizeof(string), "LEÑADOR: {FFFFFF}Compro una motosierra por {2ECC71}$%d.", CSAW_PRICE);
   	SendClientMessage(playerid, 0x3498DBFF, string);
	return 1;
