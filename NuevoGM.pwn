public OnGameModeExit()
{
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i) && PlayerData[i][Registrado] == true)
		{
			GuardarCuenta(i);
			SendClientMessageEx(i,COLOR_ROJO,"* Cuentas guardadas.");
			new query[256];
			format(query,sizeof(query),"SELECT `Nombre` FROM `Usuarios` WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(Nombre(i)));
			new DBResult:resultado = db_query(DataBaseDB,query);
			if(db_num_rows(resultado))
			{
				db_query(DataBaseDB,"BEGIN");
				userinactivo[i] = 0;
				userinactivo[i] = gettime();
				format(query,sizeof(query),"UPDATE `Usuarios` SET `Inactividad` = '%d', `Coneccion` = '0' WHERE `Nombre` = '%s' COLLATE NOCASE",userinactivo[i],DB_Escape(Nombre(i)));
				db_query(DataBaseDB,query);
				db_query(DataBaseDB,"COMMIT");
			}db_free_result(resultado);
		}
	}
	db_close(DataBaseDB);
	KillTimer(Timer@Guardar@Usuarios);
	KillTimer(WeatherTimer);
	foreachEx(z:ZONAS_MAXIMAS)
	{
		if(Zonas[z][Existe] == 1)
		{
			GangZoneDestroy(Zonas[z][ID]);
			Area_Delete(RectangleZonas[z]);
		}
	}
	for(new i = 0; i < sizeof(iPickups); i++)
	{
	    DestroyObject(iPickups[i][0]);
        DestroyPickup(iPickups[i][1]);
        DestroyPickup(iPickups[i][2]);
        DestroyPickup(iPickups[i][3]);
        DestroyPickup(iPickups[i][4]);
	}
	foreach(new i : Player)
	{
	    Player_ResetCutting(i);
		Player_RemoveLog(i);

		DestroyPlayerProgressBar(i, CuttingBar[i]);
	}
	foreach(new i : Buyers) DestroyDynamicActor(BuyerData[i][buyerActorID]);
	db_close(DataBaseDB);
	return 1;
}

