COMMAND:misgarages(playerid)
{
	new lista[ (200) * 4 ] , ide , distancia[ 100 ] ;/* PONER 5 PARA PREVENIR UN POSIBLE BUG */
	new puede = sizeof(Garages) , lleva = 0;
	switch(PlayerData[playerid][Vip])
	{
	    case 0: puede = 1;
	    case 1: puede = 2;
	    case 2: puede = 3;
	    case 3: puede = 4;
	    case 4: puede = 5;
	}/* MOSTRAR LAS BODEGAS */
	foreachEx(i:sizeof(Garages))
	{
		if(Garages[i][bo@usada] && Garages[i][bo@vendida] && strcmp(Garages[i][bo@dueno],Nombre(playerid),false)==0)
		{
			lleva++;
			if(lleva <= puede)
			{/* 3999.0 POR QUE NO SE DE QUE TAMAÑO SEA UNA FUTURA BODEGA */
				if(IsPlayerInRangeOfPoint(playerid,3999.0,Garages[i][bo@spos][0],Garages[i][bo@spos][1],Garages[i][bo@spos][2]) && GetPlayerVirtualWorld(playerid) == Garages[i][bo@mundo] && GetPlayerInterior(playerid) == Garages[i][bo@interior] )
				{
					format(distancia,sizeof(distancia),"{7FFFD4}Estas dentro del garage.");
				}
				else
				{
					format(distancia,sizeof(distancia),"%s",ObtenerDistancia(playerid,Garages[i][bo@epos][0],Garages[i][bo@epos][1],Garages[i][bo@epos][2]));
				}
				format(lista,sizeof(lista),"%s\nGarage %d - Dis:%s\n",lista, ( ide + 1 ) , distancia);
				selecciono_casa[playerid][ide] = i;
				ide += 1;
			}
		}
	}
	if(lleva > puede)
	{
		SendClientMessage(playerid,COLOR_ROJO,"Parece que tienes algunos Garages bloqueadas debido a tu nivel de cuenta vip.");
	}
	if(ide)
	{
		ShowPlayerDialog(playerid,DIALOGO_MISBODEGAS,DIALOG_STYLE_LIST,"{ffffff}Mis Garages",lista,"Siguiente","Salir");
	}else return SendClientMessage(playerid,COLOR_ROJO,"Usted no tiene Garages.");
	return true;
}

COMMAND:comprargarage(playerid)
{/*COMPRAR BODEGAS*/
	new contar = 0;/* LIMITAR BODEGAS */
	foreachEx(i:sizeof(Garages))
	{
		if(Garages[i][bo@usada] && Garages[i][bo@vendida])
		{
			if(strcmp(Garages[i][bo@dueno],Nombre(playerid),false)==0)
			{
				contar++;
			}
		}
	}
	switch(PlayerData[playerid][Vip])
	{
		case 0:if(contar >= 1)return MensajeError(playerid, "Solo puedes tener 1 garage.");
		case 1:if(contar >= 2)return MensajeError(playerid, "Solo puedes tener 2 Garages.");
		case 2:if(contar >= 3)return MensajeError(playerid, "Solo puedes tener 3 Garages.");
		case 3:if(contar >= 4)return MensajeError(playerid, "Solo puedes tener 4 Garages.");
		case 4:if(contar >= 5)return MensajeError(playerid, "Solo puedes tener 5 Garages.");
	}/* NEXT */
	foreachEx(i:sizeof(Garages))
	{
		if(IsPlayerInRangeOfPoint(playerid,8.0,Garages[i][bo@epos][0],Garages[i][bo@epos][1],Garages[i][bo@epos][2]))
		{
			if(!Garages[i][bo@vendida])
			{
				new valor = Garages[i][bo@precio];
				if(PlayerData[playerid][Vip] >= 4)valor = ((Garages[i][bo@precio]/100)*80);
				if(PlayerData[playerid][Dinero] < valor)return MensajeError(playerid,"Sin suficientes dinero.");
				PlayerData[playerid][Dinero] -= valor;
				Clanes[PlayerData[playerid][cClanid]][clNomina] += (Garages[i][bo@precio] * 5);
				format(Garages[i][bo@dueno],MAX_PLAYER_NAME,"%s",Nombre(playerid));
				format(Garages[i][bo@clave],MAX_CLAVE_BODEGA,"garage%d",i);
				format(Garages[i][bo@nombre],MAX_NOMBRE_BODEGA,"Garage de %s",Nombre(playerid));
				Garages[i][bo@puerta] = 1;
				Garages[i][bo@vendida] = 1;
				Garages[i][bo@usada] = 1;
				Garages[i][bo@dinero] = 0;
				guardarBodega(i);
				SendClientMessage(playerid,COLOR_VERDE,"Garage comprado.");
				SendSms(Nombre(playerid),"Soporte","La garage fue comprada con exito.~n~Puedes controlar tus Garages usando el comando ~y~/misbodegas~n~~w~Buen dia.");
			}
		}
	}return true;
}

