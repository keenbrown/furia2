COMMAND:crearclan(playerid,params[])
{
	if(!tieneClan(playerid))
	{
		if(ContarClanes() >= CLANES_MAXIMOS)return SendClientMessageEx(playerid,COLOR_ROJO,"Se alcanzo el limite de clanes {FFFFFF}[%d]",CLANES_MAXIMOS);
		if(PlayerData[playerid][Coins] < 25)return SendClientMessage(playerid,COLOR_ROJO,"Necesitas 25 coins para crear el clan.");
		if(PlayerData[playerid][Nivel] < 10)return SendClientMessage(playerid,COLOR_ROJO,"Necesitas ser nivel 10 para crear el clan.");
		ShowPlayerDialog(playerid,DIALOGO_NOMBRE_CLAN,DIALOG_STYLE_INPUT,""colorlima"Escriba un nombre para su clan","{FFFFFF}Escriba el nombre que desea tener en su clan","Siguiente","Cancelar");
	}else return ShowError(playerid,"Usted ya pertenece a un clan.");
	return 1;
}

COMMAND:darrango(playerid,params[])
{// /darrango
	new Jugador,eRango;
	if(tieneClan(playerid))
	{
		if(Clan[playerid][Creador] == 1 || Clan[playerid][Lider] == 1)
		{
			if(sscanf(params,"ud",Jugador,eRango))return ShowError(playerid,"Usa: /darrango [Nombre - ID] [Rango]");
			if(IsPlayerConnected(Jugador))
			{
				if(eRango >= 1 && eRango <= 7)
				{
					if(Clan[Jugador][Miembro] == Clan[playerid][Miembro])
					{
						if(Clan[Jugador][Creador] || Clan[Jugador][Lider])return ShowError(playerid,"No puedes modificar el rango de un Lider.");
						Clan[Jugador][Rango] = eRango;
						SendClientMessageEx(Jugador,COLOR_AZULCLARO,"El lider %s te dio el rango "coloramarillo"%s[%d]",Nombre(playerid),ObtenerRango(Clan[playerid][Miembro],eRango),Rango);
						new clanid = Clan[playerid][Miembro];
						foreach(new i : Player)
						{
							if(IsPlayerConnected(i))
							{
								if(clanid == Clan[i][Miembro])
								{
									SendClientMessageEx(i,COLOR_AZULCLARO,"« CLAN » {ffffff}%s cambio el rango de %s en el clan [%s] - "coloramarillo"Nuevo rango: [%s]",Nombre(playerid),Nombre(Jugador),Clanes[clanid][aNombre],ObtenerRango(clanid,Clan[Jugador][Rango]));
								}
							}
						}
					}else return SendClientMessage(playerid,COLOR_ROJO,"El jugador no es de tu clan.");
				}else return SendClientMessage(playerid,COLOR_ROJO,"Solo rangos de [1 - 7]");
			}else return SendClientMessage(playerid,COLOR_ROJO,"El jugador no se encuentra conectado.");
		}else return SendClientMessage(playerid,COLOR_ROJO,"No tienes permisos para dar rango.");
	}else return SendClientMessage(playerid,COLOR_ROJO,"No perteneces a un clan.");
	return 1;
}

COMMAND:darclan(playerid,params[])
{// /darclan
	new Jugador,st[400];
	if(tieneClan(playerid))
	{
		if(Clan[playerid][Creador] == 1 || Clan[playerid][Lider] == 1 || Clan[playerid][Rango] >= 5)
		{
			if(sscanf(params,"u",Jugador))return ShowError(playerid,"Usa: /darclan [Nombre - ID]");
			if(IsPlayerConnected(Jugador))
			{
				if(Clan[Jugador][Miembro] == 0)
				{
					if(InvitacionClan[Jugador] == false)return SendClientMessage(playerid,COLOR_ROJO,"El jugador tiene desactivadas las invitaciones a clan.");
					format(st,sizeof(st),""colorblanco"El jugador %s te invito a unirse a su clan %s\n¿Deseas "colorlima"aceptarla"colorblanco" o "colorrojo"rechazarla?",Nombre(playerid),Clanes[Clan[playerid][Miembro]][aNombre]);
