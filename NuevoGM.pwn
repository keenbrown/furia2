stock number_format_thousand(e_number) /* By adri1 - SFR3 4.3 */
{
	new f_string[24], bool:negative;
	format(f_string, sizeof f_string, "%d", e_number);
	if(e_number < 0)
	{
		negative = true;
		strdel(f_string, 0, 1);
	}

	new numbers = strlen(f_string);
	while(numbers > 3)
	{
		numbers -= 3;
		strins(f_string, ".", numbers);
	}

	if(negative) strins(f_string, "-", 0);
	return f_string;
}

stock convertirKg(i)
{
	new s[29];
	if(i < 1000)
	{
		format(s, sizeof s, "%d %s",i,macro(i==1,"gramo","gramos"));
	}
	else if(i > 999)
	{
		new Float:x = i;
		format(s, sizeof s, "%0.2f %s",( x / 1000.0 ),macro(x==1,"Kilogramo","Kilogramos"));
	}return s;
}

stock ObtenerDistancia(playerid,Float:x,Float:y,Float:z)
{
	new	distancia[84],Float: fdistancia = GetPlayerDistanceFromPoint(playerid,x,y,z);
	if(fdistancia <= 999.0)
	{
		format(distancia, sizeof distancia, "{FFFFCC}%0.2fM.",fdistancia);
	}
	else if(fdistancia >= 1000.0)
	{
		new Float:distance = (fdistancia / 1000.0);
		format(distancia, sizeof distancia, "{FFFFCC}%0.2fKm.",distance);
	}

	return distancia;
}

