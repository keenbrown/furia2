COMMAND:mensaje(playerid)return cmd_mensajes(playerid);
COMMAND:mensajes(playerid)
{
	moffset[playerid]=0;
	new tmp[(23+24+48)*10+18],DBResult:r , leido , envia[24] , fecha[48] , i = 0, id = 0;
	format(tmp, sizeof tmp, "SELECT * FROM mensajes WHERE recibe='%s' ORDER BY ID DESC LIMIT 10",Nombre(playerid));
	r=db_query(DataBaseDB,tmp);
	tmp = "";
	if(db_num_rows(r))
	{
		do
		{
			id = db_get_field_assoc_int(r,"ID");
			selectsms[playerid][i] = id;
			i++;
			leido = db_get_field_assoc_int(r,"leido");
			db_get_field_assoc(r,"envia",envia,24);
			db_get_field_assoc(r,"fecha",fecha,sizeof fecha);
			format(tmp, sizeof tmp, "%s%sMensaje de %s[%s]\n",tmp,macro(leido==0,"{FFD700}[Nuevo]","{ffffff}"),envia,fecha);
		}
		while(db_next_row(r));
		strcat(tmp, "Cerrar\n");
		selectsms[playerid][i] = -100;
		i++;
		/*Eliminar mensajes*/
		strcat(tmp, "Eliminar todos mis mensajes\n");
		selectsms[playerid][i] = -102;
		i++;
		//
		if(i >= 10)
		{
			strcat(tmp,"Siguiente");
			selectsms[playerid][i] = -101;
		}
		ShowPlayerDialog(playerid,DIALOGO_MENSAJES,DIALOG_STYLE_LIST,"{FFFFFF}Buzon de entrada",tmp,"Leer","Eliminar");
	}db_free_result(r);
	return true;
}

stock ObtenerHora()
{
	new timed[15],ti[2];
	gettime(ti[0],ti[1]);
	format(timed,sizeof(timed),"%d:%d",ti[0],ti[1]);
	return timed;
}

stock SendSms(recibe[],envia[],sms[])
{
	new tmp[(95+24+24+512+20+20)];
	format(tmp, sizeof tmp, "INSERT INTO mensajes (envia,recibe,leido,fecha,texto) VALUES ('%s','%s','0','%s a las %s','%s')",envia,recibe,ObtenerFecha(),ObtenerHora(),sms);
	db_query(DataBaseDB,tmp);
	format(tmp, sizeof tmp, "Recibio un nuevo mensaje de %s",envia);
	foreachEx(i:GetPlayerPoolSize()+1)if(IsPlayerConnected(i)&&strcmp(Nombre(i),recibe,false)==0)SendClientMessage(i,COLOR_VERDE,tmp);
}
stock msgnoLeido(playerid)
{
	new tmp[99],DBResult:r;
	format(tmp,sizeof tmp, "SELECT * FROM mensajes WHERE recibe='%s' AND leido='0'",Nombre(playerid));
	r=db_query(DataBaseDB,tmp);
	return db_num_rows(r);
}
