public OnPlayerDisconnect(playerid, reason)
{
	if (lstream[playerid])
    {
        SvDeleteStream(lstream[playerid]);
        lstream[playerid] = SV_NULL;
    }
	GuardarArmaEnDB(playerid);
	KillTimer(Tiempomuteado[playerid]);
	KillTimer(ComenzarTiempo[playerid]);
	KillTimer(peajetimer);
	ResetSweeperInfo(playerid);
	Player_CleanUp(playerid, 1);
	PlayerData[playerid][activity_afk] = 0;
	PlayerData[playerid][delay_time_afk] = 0;

	Player_ResetCutting(playerid);
	Player_RemoveLog(playerid);

	EditingTreeID[playerid] = -1;

	StopGPSPepe(playerid);
	
	new dsada[GPSPepe_Enum];
	GPSPepe[playerid] = dsada;
	if(PlayerData[playerid][cCoche] != -1)
	{
		deleteVehicle(PlayerData[playerid][cCoche]);
	}
	new query[156];
	userinactivo[playerid] = 0;
	userinactivo[playerid] = gettime();
	format(query,sizeof(query),"UPDATE `Usuarios` SET `Inactividad` = '%d' WHERE `Nombre` = '%s' COLLATE NOCASE",userinactivo[playerid],DB_Escape(Nombre(playerid)));
	db_query(DataBaseDB,query);
	//==================== ACTUALIZAR CONECCION ==============================//
	format(query,sizeof(query),"UPDATE `Usuarios` SET `Coneccion` = '0' WHERE `Nombre` = '%s' COLLATE NOCASE",DB_Escape(Nombre(playerid)));
	db_query(DataBaseDB,query);
	//========= Enviar mensaje del desconectado a los administradores ========//
	new InfoRazon[3][] =
	{
		"(Crash)",
		"(Salio)",
		"(Expulsado)"
	};
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i) && PlayerData[i][Admin] >= 1 || esRcon(i))
		{
			SendClientMessageEx(i,0x999999FF,"%s ha salido del servidor %s", Nombre(playerid), InfoRazon[reason]);
		}
	}
/*	new unconection[46];
	format(unconection,sizeof(unconection),"%s abandono el servidor.",Nombre(playerid));
	_AnunciosGTA5(unconection);
*/
	foreachEx(x:50)//=== Desactivar contador de tiempo para comandos VIP =====//
	{
		ChecarComandoVIP[x][playerid] = 0;
	}
	GuardarCuenta(playerid);
//	SavePos(playerid);
	GuardarVehiculo(playerid);

	PMS[playerid] = true;
	BigEar[playerid] = false;
	enModoespia[playerid] = false;
	en@carcel[playerid] = false;
	RemovePizza(playerid);
	tiempo@carcel[playerid] = 0;
	minutos@carcel[playerid] = 0;
	
 	if(HasTrash[playerid]) Trash_ResetPlayer(playerid);

	return 1;
}

