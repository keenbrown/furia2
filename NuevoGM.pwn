stock IsRangePointToPoint(Float:x,Float:y,Float:z,Float:x2,Float:y2,Float:z2,Float:r)
{
	new Float:np[3];
	np[0] = (x - x2);
	np[1] = (y - y2);
	np[2] = (z - z2);
	if( ((np[0] < r) && (np[0] > -r)) && ((np[1] < r) && (np[1] > -r)) && ((np[2] < r) && (np[2] > -r)))
	{
		return true;
	}return false;
}

