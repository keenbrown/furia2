        if(IsValidVehicle(trailerid))
        {
            if(!g_PlayerTrailerState[playerid])// Si el remolque está enganchado y no estaba registrado como enganchado previamente
            {
                ShowInfo(playerid, "Trailer enganchado.");
                g_PlayerTrailerState[playerid] = 1; // Establecer el estado del remolque como enganchado
