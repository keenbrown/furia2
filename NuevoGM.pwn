forward UpdateGarmin(playerid, Float:X, Float:Y, Float:Z); public UpdateGarmin(playerid, Float:X, Float:Y, Float:Z)
{
	if(GetPlayerInterior(playerid) != 0)return StopGPSPepe(playerid);
	if(IsPlayerInRangeOfPoint(playerid, 30.0, GPSPepe[playerid][GPSPepe_TickPosition][0], GPSPepe[playerid][GPSPepe_TickPosition][1], GPSPepe[playerid][GPSPepe_TickPosition][2])) return true;		// Prevent Lags
	new	MapNode:start,MapNode:target;
	GetPlayerPos(playerid, GPSPepe[playerid][GPSPepe_TickPosition][0], GPSPepe[playerid][GPSPepe_TickPosition][1], GPSPepe[playerid][GPSPepe_TickPosition][2]);
    if(GetClosestMapNodeToPoint(GPSPepe[playerid][GPSPepe_TickPosition][0], GPSPepe[playerid][GPSPepe_TickPosition][1], GPSPepe[playerid][GPSPepe_TickPosition][2], start) != 0) return true;
    if(GetClosestMapNodeToPoint(X, Y, Z, target)) return true;
	if(FindPathThreaded(start, target, "OnPlayerGarminRouters", "i", playerid)) return true;
    return true;
}

forward OnPlayerGarminRouters(Path:pathid, playerid); public OnPlayerGarminRouters(Path:pathid, playerid)
{
    if(!IsValidPath(pathid)) return true;
	if(GPSPepe[playerid][GPSPepe_Timer] == 0) return true;
    DestroyGarminRoutesGPS(playerid);
	new	Float:length, size;
	GetPathSize(pathid, size);
	GetPathLength(pathid, length);

	if(size == 1) return StopGPSPepe(playerid);
	new	MapNode:nodeid,Float:x,Float:y,Float:z,index;
	GetPlayerPos(playerid, x, y, z);
	GetClosestMapNodeToPoint(x, y, z, nodeid);
	GetMapNodePos(nodeid, x, y, z);

	new	_size = MAX_GARMIN_DOTS,Float:X,Float:Y,Float:Z;
	if(MAX_GARMIN_DOTS > size) _size = size;
	for(new i = 0; i < _size; i++)
	{
		GetPathNode(pathid, i, nodeid);
		GetPathNodeIndex(pathid, nodeid, index);
		GetMapNodePos(nodeid, X, Y, Z);
		if(i == index)CreateGarminPointer(playerid, x, y, X, Y, GPSPepe[playerid][GPSPepe_Color]);
		x = X + 0.5;
		y = Y + 0.5;
	}   return true;
}

