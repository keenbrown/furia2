stock RGBAToARGB(col)
{
	return ((((col) << 24) & 0xFF000000) | (((col) >>> 8) & 0xFFFFFF));
}

stock HexToInt(string[])
{
	if(!string[0])
  	{
  		return 0;
	}
  	new cur = 1, res = 0;
  	for(new i = strlen(string); i > 0; i--)
 	{
  		if(string[i - 1] < 58)
  		{
		  	res= res + cur * (string[i - 1] - 48);
  		}
  		else
  		{
	  		res = res + cur * (string[i - 1] - 65 + 10);
		}
    	cur = cur * 16;
  	}	return res;
}

