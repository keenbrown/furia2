static stock DestroyGarminRoutesGPS(playerid)
{
	for(new i; i < MAX_GARMIN_DOTS; i++)
	{
		if(GPSPepe[playerid][GPSPepe_Routes][i] == -1) continue;
		GangZoneDestroy(GPSPepe[playerid][GPSPepe_Routes][i]);
		GPSPepe[playerid][GPSPepe_Routes][i] = -1;
	}
}

static stock CreateGarminPointer(playerid, Float:X1, Float:Y1, Float:X2, Float:Y2, color)
{
	new Float:Dis = 12.5, Float:x, Float:y, Points = floatround(GDBP(X1, Y1, 0.0, X2, Y2, 0.0) / Dis);
	for(new i = 1; i <= Points; i++)
	{
        switch(i)
		{
            case 0:
			{
                x = X1;
			    y = Y1;
            }
            default:
			{
                x = X1 + (((X2 - X1) / Points) * i);
			    y = Y1 + (((Y2 - Y1) / Points) * i);
            }
        }
		new slot = 0;
		while(slot <= MAX_GARMIN_DOTS)
		{
			if(slot == MAX_GARMIN_DOTS)
			{
				slot = -1;
				break;
			}
			if(GPSPepe[playerid][GPSPepe_Routes][slot] == -1) break;
			slot++;
		}
		if(slot == -1) return;
        GPSPepe[playerid][GPSPepe_Routes][slot] = GangZoneCreate(x - (Dis / 2)-  5, y - (Dis / 2) - 5, x + (Dis / 2) + 5, y + (Dis / 2) + 5);
		GangZoneShowForPlayer(playerid, GPSPepe[playerid][GPSPepe_Routes][slot], color);
	}   return;
}
