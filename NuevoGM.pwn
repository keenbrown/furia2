	{
		if(sscanf(usr,"u",usr[0]))return ShowError(playerid,"Usa: /arrestar [Usuario]");
		new i = usr[0];
		if(IsPlayerConnected(i))
		{
			if(GetPlayerWantedLevel(i) < 1)return ShowError(playerid,"El jugador no tiene cargos.");
			if(IsPlayerInRangeOfPoint(i,5.0,pos[0],pos[1],pos[2])){
