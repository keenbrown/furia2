COMMAND:pm(playerid,params[])
{// /PM
	new Jugador,Mensaje[128];
	if(sscanf(params,"us[128]",Jugador,Mensaje))return SendClientMessage(playerid,COLOR_ROJO,"Modo de uso: /pm [Nombre - ID] [Mensaje]");
	if(PlayerData[Jugador][activity_afk] > 2)return SendClientMessage(playerid,COLOR_ROJO,"El usuario esta en pausa, espera a que regrese al juego.");
	if(!IsPlayerConnected(Jugador))return SendClientMessage(playerid,COLOR_ROJO,"El jugador no se encuentra conectado.");
	if(PMS[Jugador] == false)return SendClientMessage(playerid,COLOR_ROJO,"El jugador tiene desactivados los mensajes privados.");
	if(Jugador == playerid)return SendClientMessage(playerid,COLOR_ROJO,"No puedes enviarte mensajes privados a ti mismo.");
	if(PlayerData[playerid][Silenciado] == 1) return SendClientMessage( playerid, COLOR_ROJO, "* Usted se encuentra silenciado." );
	//------------------------------------------------------------------------------
	SendClientMessageEx(Jugador,COLOR_AMARILLO,"PM de %s(%d): "colorblanco"%s",Nombre(playerid),playerid,Mensaje);
	PlayerPlaySound(Jugador, 1057, 0.0, 0.0, 0.0);
	GameTextForPlayer(Jugador,"_~n~_~n~_~n~~g~~h~Nuevo Mensaje Recibido",4000,6);
	//------------------------------------------------------------------------------
	PlayerPlaySound(playerid, 1057, 0.0, 0.0, 0.0);
	SendClientMessageEx(playerid,COLOR_AMARILLO,"PM enviado a %s(%d): "colorblanco"%s",Nombre(Jugador),Jugador,Mensaje);
	GameTextForPlayer(playerid,"_~n~_~n~_~n~~w~~h~Mensaje ~r~~h~Enviado",6000,6);
	//------------------------------------------------------------------------------
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i) && PlayerData[i][Admin] >= 1 || esRcon(i))
		{
			if(LeerPMS == true && PlayerData[i][Admin] >= PlayerData[playerid][Admin] && !esRcon(playerid) || !esFundador(playerid))
