COMMAND:armas(playerid)
{
    new query[256], slot[MAX_WEAPONS], arma, municiones,dialogtext[1256];
    dialogtext[0] = EOS;
    for (new i = 0; i < MAX_WEAPONS; i++)
    {
        format(query, sizeof(query), "SELECT `Arma`, `Mun` FROM `ArmasPlayer` WHERE `Nombre` = '%s' AND `Slot` = %d", Nombre(playerid), i);
        new DBResult:result = db_query(DataBaseDB, query);
        if(db_num_rows(result))
        {
            db_next_row(result);
            slot[i] = i;
            arma = db_get_field_int(result, 0);
            municiones = db_get_field_int(result, 1);
            format(dialogtext, sizeof(dialogtext), "%s\nSlot %d: %s (Municiones: %d)", dialogtext, i, Armas@Jugador[arma], municiones);
        }
        else
        {
            slot[i] = -1;
            format(dialogtext, sizeof(dialogtext), "%s\nSlot %d: Nada", dialogtext, i);
        }
        db_free_result(result);
    }
    new selection = ShowPlayerDialog(playerid, DIALOG_WEAPON_LIST, DIALOG_STYLE_LIST, "Armas Guardadas", dialogtext, "Seleccionar", "Cancelar");
    if(selection)
    {
        new selected_slot = selection - 1; // Los índices de los slots comienzan desde 0
        if(selected_slot >= 0 && selected_slot < MAX_WEAPONS && slot[selected_slot] != -1)
        {
            GivePlayerWeapon(playerid, slot[selected_slot], municiones); // Se le otorga al jugador 1 unidad del arma del slot seleccionado
            SendClientMessageEx(-1, COLOR_VERDECLARO, "¡Has recibido el arma del Slot %d!", selected_slot);
        }
    }   return 1;
}

