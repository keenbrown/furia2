stock UsuarioEnZona(playerid)
{
	new zonaid = 0, Float:px, Float:py, Float:pz;
	foreachEx(zoneid:ZONAS_MAXIMAS)
	{
		if(Zonas[zoneid][Existe] == 1)
		{
			if(GetPlayerState(playerid) != PLAYER_STATE_WASTED)
			{
				GetPlayerPos(playerid, px, py, pz);
				if(px > Zonas[zoneid][zPos][0] - ZONA_LONGITUD && py > Zonas[zoneid][zPos][1] - ZONA_LONGITUD && px < Zonas[zoneid][zPos][0] + ZONA_LONGITUD && py < Zonas[zoneid][zPos][1] + ZONA_LONGITUD)
				{
					zonaid = zoneid;
				}
			}
		}
	}return zonaid;
}

Funcion.Update@CapturaZone(zoneid,t)
{
	if(Zonas[zoneid][Conquistando] == 1 && Zonas[zoneid][Conquistador@actual] == t)
	{
		if(ObtenerMiembrosEnZona(zoneid,t) < 0)
		{
			SendClientMessageEx(-1,coloresclanlista[Clanes[t][aColor]],"« ZONAS » {FFFFFF} El clan %s fallo al capturar una zona - "colorkaki"[Zonas: %d/%d]",Clanes[t][aNombre],Clanes[t][aConquista],ContarZonas());
			foreachEx(i:MAX_PLAYERS)
			{
				if(IsPlayerConnected(i)&&Zona@Time@Restante[i] == zoneid)
				{
					PlayerTextDrawHide(i,Zonas@Tiempo@Restante@Conquista);
					Zona@Time@Restante[i] = 0;
					GangZoneStopFlashForAll(Zonas[zoneid][ID]);
					Zonas[zoneid][Conquistando] = 0;
					Zonas[zoneid][Conquistador@actual] = 0;
					KillTimer(TimerZona[zoneid]);
				}
			}return 0;
		}
		else
		{
			new quer[86];
			// Remover stats al clan anterior //
			Clanes[Zonas[zoneid][Dueno]][aConquista]--;
			Clanes[Zonas[zoneid][Dueno]][aScore] --;
			db_query(DataBaseDB,"BEGIN");
			format(quer,sizeof(quer),"UPDATE Clanes SET Conquistas = Conquistas-1 WHERE ID = '%d' COLLATE NOCASE",Zonas[zoneid][Dueno]);db_query(DataBaseDB,quer);
			format(quer,sizeof(quer),"UPDATE Clanes SET Score = Score-1 WHERE ID = '%d' COLLATE NOCASE",Zonas[zoneid][Dueno]);db_query(DataBaseDB,quer);
			db_query(DataBaseDB,"COMMIT");

			//Asignar al nuevo dueño de la zona.
			Zonas[zoneid][Dueno] = t;
			db_query(DataBaseDB,"BEGIN");
			format(quer,sizeof(quer),"UPDATE `Zonas` SET `Dueno` = '%d' WHERE `ID` = '%d' COLLATE NOCASE",t,zoneid);db_query(DataBaseDB,quer);
			db_query(DataBaseDB,"COMMIT");

			GangZoneStopFlashForAll(Zonas[zoneid][ID]);
			GangZoneShowForAll(Zonas[zoneid][ID],coloresclanlista[Clanes[Zonas[zoneid][Dueno]][aColor]]);
			Clanes[Zonas[zoneid][Dueno]][aConquista]++;
			Clanes[Zonas[zoneid][Dueno]][aScore] ++;
			db_query(DataBaseDB,"BEGIN");
			format(quer,sizeof(quer),"UPDATE Clanes SET Conquistas = Conquistas+1 WHERE ID = '%d' COLLATE NOCASE",Zonas[zoneid][Dueno]);db_query(DataBaseDB,quer);
			format(quer,sizeof(quer),"UPDATE Clanes SET Score = Score+1 WHERE ID = '%d' COLLATE NOCASE",Zonas[zoneid][Dueno]);db_query(DataBaseDB,quer);
			db_query(DataBaseDB,"COMMIT");

			SendClientMessageEx(-1,coloresclanlista[Clanes[Zonas[zoneid][Dueno]][aColor]],"« ZONAS » {FFFFFF} El clan %s capturo una zona "colorkaki"[Zonas: %d/%d].",Clanes[Zonas[zoneid][Dueno]][aNombre],Clanes[Zonas[zoneid][Dueno]][aConquista],ContarZonas());
			Zonas[zoneid][Conquistando] = 0;
			Zonas[zoneid][Conquistador@actual] = 0;
			KillTimer(TimerZona[zoneid]);

			//Remover textdraws de la zona.
			foreachEx(i:MAX_PLAYERS)
			{
				if(IsPlayerConnected(i)&&Zona@Time@Restante[i] == zoneid)
				{
					PlayerTextDrawHide(i,Zonas@Tiempo@Restante@Conquista);
					Zona@Time@Restante[i] = 0;
				}
			}//Regalar $1200 & 3 score a los que se encuentren en zona
			foreachEx(i:MAX_PLAYERS)
			{
				if(IsPlayerConnected(i) && UsuarioEnZona(i) == zoneid)
				{
					PlayerData[i][Dinero] += 1200;
					PlayerData[i][Score] += 3;
					TextDrawShowForPlayer(i,RecompensaClanes[i]);
					TextDrawShowForPlayer(i,Recompensa@Clanes[i]);
					SetTimerEx("Destruir@Info", 3000, false, "d", i);
				}
			}
		}
	}	return 1;
}

stock SlotZonaDisponible()
{// Ejemplo: si hay 10 zonas y la id salta del [2] a [4] - La id 3 esta disponible, entonces aqui entra esta funcion, selecciona esa id.
	new slot = 0, count = 0, ide;
	new DBResult:resultado = db_query(DataBaseDB,"SELECT `ID` FROM `Zonas` WHERE `ID` > 0");
	if(db_num_rows(resultado))
	{
		do
		{
			count++;
			ide = db_get_field_int(resultado,0);
			if(ide != count)
			{
				slot = count;
				printf("%d ide - %d count",ide,count);
				return slot;
			}
		}while(db_next_row(resultado));
	}db_free_result(resultado);
	return slot;
}

Funcion.CrearZonaEx(playerid,zone,time)
{
	new Float:Pos[3],zono[MAX_ZONE_NAME];
	GetPlayer2DZone(playerid,zono,MAX_ZONE_NAME);
	GetPlayerPos(playerid,Pos[0],Pos[1],Pos[2]);
	new DBResult:resultado = db_query(DataBaseDB,"SELECT * FROM `Zonas` WHERE `ID` > 0");
	if(db_num_rows(resultado))
	{
		do
		{
			new chk[120],Float:zona_Pos[3];
			db_get_field_assoc(resultado,"Pos_X",chk,sizeof(chk));
			zona_Pos[0] = floatstr(chk);

			db_get_field_assoc(resultado,"Pos_Y",chk,sizeof(chk));
			zona_Pos[1] = floatstr(chk);

			db_get_field_assoc(resultado,"Pos_Z",chk,sizeof(chk));
			zona_Pos[2] = floatstr(chk);

			if(IsPlayerInRangeOfPoint(playerid,ZONA_LONGITUD,zona_Pos[0],zona_Pos[1],zona_Pos[2]))
			{
				SendClientMessage(playerid,COLOR_ROJO,"Estas muy cerca de una zona conquistable ya existente.");
				return 1;
			}
		}while(db_next_row(resultado));
	}db_free_result(resultado);

	new Query[580];
	format(Query,sizeof(Query),"INSERT INTO `Zonas` (`ID`,`Pos_X`,`Pos_Y`,`Pos_Z`,`Dueno`,`Zonaname`,`Existe`,`Tiempoconquista`) VALUES ('%d','%f','%f','%f','0','%s','1','%d')",zone,Pos[0],Pos[1],Pos[2],zono,time);
	db_query(DataBaseDB,Query);

	Zonas[zone][zPos][0] = Pos[0];
	Zonas[zone][zPos][1] = Pos[1];
	Zonas[zone][zPos][2] = Pos[2];
	format(Zonas[zone][Ubicacion],50,"%s",zono);
	Zonas[zone][Tiempoconquista] = time;
	Zonas[zone][Existe] = 1;

	Zonas[zone][ID] = GangZoneCreate(Zonas[zone][zPos][0] - ZONA_LONGITUD,Zonas[zone][zPos][1] - ZONA_LONGITUD,Zonas[zone][zPos][0] + ZONA_LONGITUD,Zonas[zone][zPos][1] + ZONA_LONGITUD);//Crear la GanZone.
	RectangleZonas[zone] = Area_AddBox(Zonas[zone][zPos][0] - ZONA_LONGITUD,Zonas[zone][zPos][1] - ZONA_LONGITUD,Zonas[zone][zPos][0] + ZONA_LONGITUD,Zonas[zone][zPos][1] + ZONA_LONGITUD);

	SendClientMessageEx(-1,COLOR_AMARILLO,"* El Admin %s creo una zona conquistable {FFFFFF}[Zona: %s - Conquista: %d Minuto's]",Nombre(playerid),zono,time);

	//Mostrara las GangZones
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i))
		{
			MostrarZonas(i);
		}
	}return 1;
}

