public OnPlayerEnterCheckpoint(playerid)
{
	if(HasSetCheckpoint[playerid] == true)
 	{
    	DisablePlayerCheckpoint(playerid);
    	HasSetCheckpoint[playerid] = false;
    }
    if(Ubicacion@GPS[playerid])
	{
        DisablePlayerCheckpoint(playerid);
        Ubicacion@GPS[playerid] = false;
    }
    if(Ubicacion@Gasolinera[playerid] == true)
	{
		if(CamionLleno[playerid] == true)
		{
  			Ubicacion@Gasolinera[playerid] = false;
	        CamionLleno[playerid] = false;
	        DisablePlayerCheckpoint(playerid);
        }
    }
   	if(PizzaPlayer[playerid])
 	{
		if(PizzaInHands[playerid])
		{
		    new string[5];
		    new pay = PIZZA_MONEY;
	    	SetPizzaCheckpoint(playerid);
	    	RemovePizza(playerid);
			PizzaCash[playerid] += pay;
			format(string,5,"+%d",pay);
			GameTextForPlayer(playerid, string, 2000, 4);
	    }
		else
		{
	        SendClientMessage(playerid,PIZZA_COLOR,"Te olvidaste de la pizza!");
	    }
    }
    return 1;
}

