COMMAND:encerrar(playerid,params[])
{// /encerrar
	if(PlayerData[playerid][Admin] >= 4 || esRcon(playerid) || esFundador(playerid))
	{
		new Jugador,Minutos,Razon[35],string[150];
		if(sscanf(params,"uds[35]",Jugador,Minutos,Razon))return ShowError(playerid,"Usa: /encerrar [Nombre - ID] [Minutos] [Razon]");
		if(IsPlayerConnected(Jugador))
		{
			if(PlayerData[Jugador][Admin] > PlayerData[playerid][Admin] )return SendClientMessage(playerid,COLOR_ROJO,"No puedes usar este comando con alguien superior a ti.");
			if(Minutos < 1 && Minutos > 10)return SendClientMessage(playerid,COLOR_ROJO,"Solo minutos de 1 - 10");
			if(en@carcel[Jugador]==true)return SendClientMessage(playerid,COLOR_ROJO,"El jugador ya esta encarcelado.");

			en@carcel[Jugador] = true;
			tiempo@carcel[Jugador] = gettime() + 60*Minutos;
			minutos@carcel[Jugador] = Minutos;
			timer@carcel[playerid] = SetTimerEx("LiberarDePricion",minutos@carcel[Jugador] * 60 * 1000,false,"i",Jugador);
			SetTimerEx("EnviarPrision",1000,false,"i",Jugador);

			format(string,sizeof(string),""colorrojo"¡Encarcelado!\n\n"colorblanco"Te encarcelo: "colorazul"%s\n"colorblanco"Razon: "colorazul"%s\n"colorblanco"Minutos: "colorazul"%d",Nombre(playerid),Razon,Minutos);
			ShowPlayerDialog(Jugador,DIALOGO_UNO,DIALOG_STYLE_MSGBOX,""colorrojo"Encerrado",string,"Aceptar","");

			new Query[156];
			format(Query,sizeof(Query),"INSERT INTO `Encarcelados` (`Nombre`,`Minutos`) VALUES ('%s','%d')",DB_Escape(Nombre(Jugador)),Minutos);
			db_query(DataBaseDB,Query);

			SendClientMessageEx(-1,COLOR_AZULCLARO,"* El administrador %s encarcelo a %s por %d minutos "colorblanco"(Razon: %s)",Nombre(playerid),Nombre(Jugador),Minutos,Razon);
			adminRegistros(playerid,"encerrar");

		}else return SendClientMessage(playerid,COLOR_ROJO,"El jugador no se encuentra conectado.");
	}else return SendClientMessage(playerid,COLOR_ROJO,"No tienes permisos para usar este comando.");
	return 1;
