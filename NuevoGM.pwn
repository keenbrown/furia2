AddCarTuning(vehicleid)
{
    ChangeVehicleColor(vehicleid, VehicleInfo[vehicleid][vehicle_colour1], VehicleInfo[vehicleid][vehicle_colour2]);
    if(VehicleInfo[vehicleid][EMod8]  > 0) 	 AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod8]);
	if(VehicleInfo[vehicleid][EMod10] > 0) 	 AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod10]);
	if(VehicleInfo[vehicleid][EMod11] > 0) 	 AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod11]);
	if(VehicleInfo[vehicleid][EMod6]  > 0)
	{
        AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod6]);
        new skirt = VehicleInfo[vehicleid][EMod6];
        // X-FLOW
        if(skirt == 1030) AddVehicleComponent(vehicleid, 1031);
        else if(skirt == 1039) AddVehicleComponent(vehicleid, 1041);
        else if(skirt == 1048) AddVehicleComponent(vehicleid, 1052);
        else if(skirt == 1070) AddVehicleComponent(vehicleid, 1072);
        else if(skirt == 1057) AddVehicleComponent(vehicleid, 1063);
        else if(skirt == 1093) AddVehicleComponent(vehicleid, 1095);
        // ALIEN
        if(skirt == 1026) AddVehicleComponent(vehicleid, 1027);
        else if(skirt == 1036) AddVehicleComponent(vehicleid, 1040);
        else if(skirt == 1047) AddVehicleComponent(vehicleid, 1051);
        else if(skirt == 1069) AddVehicleComponent(vehicleid, 1071);
        else if(skirt == 1056) AddVehicleComponent(vehicleid, 1062);
        else if(skirt == 1094) AddVehicleComponent(vehicleid, 1090);
	}
	if(VehicleInfo[vehicleid][EMod3] > 0) 				AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod3]);
	if(VehicleInfo[vehicleid][EMod4] > 0) 				AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod4]);
	if(VehicleInfo[vehicleid][EMod2] > 0) 				AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod2]);
	if(VehicleInfo[vehicleid][EMod1] > 0) 				AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod1]);
	if(VehicleInfo[vehicleid][EMod12] > 0) 				AddVehicleComponent(vehicleid, VehicleInfo[vehicleid][EMod12]);
