		{
		 	PlayerData[playerid][uMinutos] -= 60;
		 	PlayerData[playerid][uHoras] ++;
		 	GuardarCuenta(playerid);
		}
	}
	*/
	if(en@afk[playerid] == true)
	{
		if(tiempo@afk[playerid][0] >= 60)
		{
			tiempo@afk[playerid][1] += 1;
			tiempo@afk[playerid][0] -= 60;
		}
	}
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		//-----|Sistema de objetivos|->
		if(PlayerData[i][cObjetivo] != -1){
			if(IsPlayerConnected(PlayerData[i][cObjetivo])){
				static Float:objpos[3];
				GetPlayerPos(PlayerData[i][cObjetivo], objpos[0], objpos[1], objpos[2]);
